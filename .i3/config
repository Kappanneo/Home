 #                                HOVER-MODE                                   #

 mode $hov {

 bindsym Mod4+c $exec $oklò_disable; mode $cnf $exec $alert $cnf; $exec echo Mod4+c >> log

 bindsym XF86PowerOff $exec $oklò_disable; mode $pow $exec $alert $pow, fullscreen disable; $exec echo XF86PowerOff >> log

 bindsym Mod4+r $exec $oklò_disable; mode $red $exec $alert $red; $exec echo Mod4+r >> log

 bindsym Mod4+z $exec $oklò_disable; mode $str $exec $alert $str; $exec echo Mod4+z >> log

 bindsym Mod4+space $exec $oklò_disable; mode $tch $exec $alert $tch & $touchpad_on; $exec echo Mod4+space >> log

 bindsym BackSpace $exec $oklò_disable; mode $def $exec $alert $wrt; $exec echo BackSpace >> log
 bindsym Delete $exec $oklò_disable; mode $def $exec $alert $wrt; $exec echo Delete >> log

 # borders
 bindsym --border button2 $no_border; $exec echo --border button2 >> log

 # display
 bindsym XF86Display $exec xrandr --output eDP1 --mode 1920x1080 --preferred; $exec echo XF86Display >> log

 # screen brightness controls
 bindsym XF86MonBrightnessUp $brightness_up; $exec echo XF86MonBrightnessUp >> log
 bindsym XF86MonBrightnessDown $brightness_down; $exec echo XF86MonBrightnessDown >> log

 # screen poweroff
 bindsym XF86ScreenSaver $exec xset dpms force off; $exec echo XF86ScreenSaver >> log

 # screenshot
 bindsym --release Print $exec i3-scrot; $exec echo --release Print >> log
 bindsym --release Mod4+Print $exec i3-scrot -w; $exec echo --release Mod4+Print >> log
 bindsym --release Mod4+Shift+Print $exec i3-scrot -s; $exec echo --release Mod4+Shift+Print >> log

 # touchpad toggle
 bindsym XF86TouchpadToggle $exec $touchpad_toggle; $exec echo XF86TouchpadToggle >> log

 # no border write
 bindsym Shift+"Super_L" $exec $oklò_disable; mode $def $no_border; $exec $alert $wrt; $exec echo Shift+"Super_L" >> log

 # no border hover
 bindsym Shift+Menu $no_border; $exec echo Shift+Menu >> log

 # where am i?
 bindsym --release "Super_L" $border, $focus_one, fullscreen disable; $exec $touchpad_off; $exec echo --release "Super_L" >> log

 # kill
 bindsym Mod1+F4 kill; $refresh_status_bar; $exec echo Mod1+F4 >> log

 # borders
 bindsym Mod4+b $border; $exec echo Mod4+b >> log
 bindsym Mod4+Shift+b $no_border; $exec echo Mod4+Shift+b >> log

 # default move
 bindsym Mod4+Shift+Up move up; $exec echo Mod4+Shift+Up >> log
 bindsym Mod4+Shift+Left move left; $exec echo Mod4+Shift+Left >> log
 bindsym Mod4+Shift+Down move down; $exec echo Mod4+Shift+Down >> log
 bindsym Mod4+Shift+Right move right; $exec echo Mod4+Shift+Right >> log

 # focus group
 bindsym Mod4+less focus child; $exec echo Mod4+less >> log
 bindsym Mod4+Shift+greater focus parent; $exec echo Mod4+Shift+greater >> log

 # fullscreen
 bindsym Mod4+f fullscreen toggle; $exec echo Mod4+f >> log
 bindsym Mod4+Shift+f $exec "sleep 0.5; xdotool key F11; sleep 0.25; i3-msg fullscreen disable"; $exec echo Mod4+Shift+f >> log

 # oklò move
 bindsym Mod4+Shift+o move up; $exec echo Mod4+Shift+o >> log
 bindsym Mod4+Shift+k move left; $exec echo Mod4+Shift+k >> log
 bindsym Mod4+Shift+l move down; $exec echo Mod4+Shift+l >> log
 bindsym Mod4+Shift+ograve move right; $exec echo Mod4+Shift+ograve >> log

 # open emacs
 bindsym Mod4+e exec $emacs; $exec echo Mod4+e >> log

 # open terminal
 bindsym Mod4+Return exec gnome-terminal; $exec echo Mod4+Return >> log

 # resize window
 bindsym Mod4+i resize shrink height 5 px or 5 ppt; $exec echo Mod4+i >> log
 bindsym Mod4+p resize  grow  height 5 px or 5 ppt; $exec echo Mod4+p >> log
 bindsym Mod4+Prior resize shrink height 5 px or 5 ppt; $exec echo Mod4+Prior >> log
 bindsym Mod4+Next resize  grow  height 5 px or 5 ppt; $exec echo Mod4+Next >> log
 bindsym Mod4+Shift+i resize shrink width  5 px or 5 ppt; $exec echo Mod4+Shift+i >> log
 bindsym Mod4+Shift+p resize  grow  width  5 px or 5 ppt; $exec echo Mod4+Shift+p >> log
 bindsym Mod4+Shift+Prior resize shrink width  5 px or 5 ppt; $exec echo Mod4+Shift+Prior >> log
 bindsym Mod4+Shift+Next resize  grow  width  5 px or 5 ppt; $exec echo Mod4+Shift+Next >> log

 # split orientation
 bindsym Mod4+v split v; exec notify-send 'vertical'; $exec echo Mod4+v >> log
 bindsym Mod4+h split h; exec notify-send 'horizontal'; $exec echo Mod4+h >> log
 bindsym Mod4+g split v; focus parent; layout toggle split; focus child; $exec echo Mod4+g >> log

 # wasd move
 bindsym Mod4+Shift+w move up; $exec echo Mod4+Shift+w >> log
 bindsym Mod4+Shift+a move left; $exec echo Mod4+Shift+a >> log
 bindsym Mod4+Shift+s move down; $exec echo Mod4+Shift+s >> log
 bindsym Mod4+Shift+d move right; $exec echo Mod4+Shift+d >> log

 # default split and move
 bindsym Mod4+control+Shift+Up focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+Up >> log
 bindsym Mod4+control+Shift+Left focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+Left >> log
 bindsym Mod4+control+Shift+Down focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+Down >> log
 bindsym Mod4+control+Shift+Right focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+Right >> log

 # files
 bindsym Mod4+control+f $exec $fm; $exec echo Mod4+control+f >> log
 bindsym Mod4+control+j $exec $fm Downloads; $exec echo Mod4+control+j >> log

 # focus all
 bindsym Mod4+control+a $focus_all; $exec echo Mod4+control+a >> log

 # move container while browsing workspaces
 bindsym Mod4+control+Tab move container to workspace next; workspace next; $exec echo Mod4+control+Tab >> log
 bindsym Mod4+control+Shift+Tab move container to workspace prev; workspace prev; $exec echo Mod4+control+Shift+Tab >> log
 bindsym Mod4+control+Home move container to workspace prev; workspace prev; $exec echo Mod4+control+Home >> log
 bindsym Mod4+control+End move container to workspace next; workspace next; $exec echo Mod4+control+End >> log
 bindsym Mod4+control+ugrave move container to workspace back_and_forth; workspace back_and_forth; $exec echo Mod4+control+ugrave >> log

 # move focused container to workspace and follow
 bindsym Mod4+control+1 move container to workspace $w1; workspace $w1; $exec echo Mod4+control+1 >> log
 bindsym Mod4+control+2 move container to workspace $w2; workspace $w2; $exec echo Mod4+control+2 >> log
 bindsym Mod4+control+3 move container to workspace $w3; workspace $w3; $exec echo Mod4+control+3 >> log
 bindsym Mod4+control+4 move container to workspace $w4; workspace $w4; $exec echo Mod4+control+4 >> log
 bindsym Mod4+control+5 move container to workspace $w5; workspace $w5; $exec echo Mod4+control+5 >> log
 bindsym Mod4+control+6 move container to workspace $w6; workspace $w6; $exec echo Mod4+control+6 >> log
 bindsym Mod4+control+7 move container to workspace $w7; workspace $w7; $exec echo Mod4+control+7 >> log
 bindsym Mod4+control+8 move container to workspace $w8; workspace $w8; $exec echo Mod4+control+8 >> log
 bindsym Mod4+control+9 move container to workspace $w9; workspace $w9; $exec echo Mod4+control+9 >> log

 # oklò split and move
 bindsym Mod4+control+Shift+o focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+o >> log
 bindsym Mod4+control+Shift+k focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+k >> log
 bindsym Mod4+control+Shift+l focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+l >> log
 bindsym Mod4+control+Shift+ograve focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+ograve >> log

 # reload configuration
 bindsym Mod4+control+r $exec "make -C ~/.i3 && i3-msg $no_border restart"; $exec echo Mod4+control+r >> log
 bindsym Mod4+control+Shift+r $exec "make -C ~/.i3 stable && i3-msg $no_border restart"; $exec echo Mod4+control+Shift+r >> log

 # resize (precise)
 bindsym Mod4+control+i resize shrink height 1 px or 1 ppt; $exec echo Mod4+control+i >> log
 bindsym Mod4+control+p resize  grow  height 1 px or 1 ppt; $exec echo Mod4+control+p >> log
 bindsym Mod4+control+Prior resize shrink height 1 px or 1 ppt; $exec echo Mod4+control+Prior >> log
 bindsym Mod4+control+Next resize  grow  height 1 px or 1 ppt; $exec echo Mod4+control+Next >> log
 bindsym Mod4+control+Shift+i resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+i >> log
 bindsym Mod4+control+Shift+p resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+p >> log
 bindsym Mod4+control+Shift+Prior resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+Prior >> log
 bindsym Mod4+control+Shift+Next resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+Next >> log

