 #                              CONFIGURATIONS                                 #

 mode $cnf {

 # options
 bindsym Mod4+c mode $def $exec $emacs ~/.i3/index.conf; $exec $alert $wrt
 bindsym Mod4+m mode $def $exec $emacs ~/.i3/modes.py; $exec $alert $wrt
 bindsym Mod4+b mode $def $exec $emacs ~/.i3/bindings.py; $exec $alert $wrt
 bindsym Mod4+i mode $def $exec $emacs ~/.gitignore; $exec $alert $wrt
 bindsym Mod4+a mode $def $exec $emacs /sudo::/usr/share/applications/; $exec $alert $wrt
 bindsym Mod4+e mode $def $exec $emacs ~/.emacs; $exec $alert $wrt
 bindsym Mod4+s mode $def $exec $emacs ~/.config/i3status/config; $exec $alert $wrt
 bindsym Mod4+z mode $def $exec $emacs ~/.zshrc; $exec $alert $wrt
 bindsym Mod4+g mode $def $exec chromium --new-window i3wm.org/docs/userguide.html; $exec $alert $wrt
 bindsym Mod4+l mode $def $exec $emacs ~/.workspaces/stamp.json; $exec $alert $wrt
 bindsym Mod4+1 mode $def $exec $emacs ~/.workspaces/$w1.json; $exec $alert $wrt
 bindsym Mod4+2 mode $def $exec $emacs ~/.workspaces/$w2.json; $exec $alert $wrt
 bindsym Mod4+r mode $def $exec $emacs ~/.config/redshift/redshift.conf; $exec $alert $wrt

 bindsym Mod1+Menu mode $hov $exec $alert $hov & $oklò_enable
 bindsym Menu mode $hov $exec $alert $hov & $oklò_enable

 bindsym XF86PowerOff mode $pow $exec $alert $pow, fullscreen disable

 bindsym Mod4+r mode $red $exec $alert $red

 bindsym Mod4+z mode $str $exec $alert $str

 bindsym Mod4+space mode $tch $exec $alert $tch & $touchpad_on

 bindsym Return mode $def $exec $alert $wrt
 bindsym BackSpace mode $def $exec $alert $wrt
 bindsym space mode $def $exec $alert $wrt
 bindsym Delete mode $def $exec $alert $wrt
 bindsym "Alt_L" mode $def $exec $alert $wrt

 # borders
 bindsym --border button2 $no_border

 # display
 bindsym XF86Display $exec xrandr --output eDP1 --mode 1920x1080 --preferred

 # screen brightness controls
 bindsym XF86MonBrightnessUp $brightness_up
 bindsym XF86MonBrightnessDown $brightness_down

 # screen poweroff
 bindsym XF86ScreenSaver $exec xset dpms force off

 # screenshot
 bindsym --release Print $exec i3-scrot

 # touchpad toggle
 bindsym XF86TouchpadToggle $exec $touchpad_toggle

 # no border write
 bindsym Shift+"Super_L" mode $def $no_border; $exec $alert $wrt

 # no border hover
 bindsym Shift+Menu mode $hov $no_border; $exec $alert $hov & $oklò_enable

 # where am i?
 bindsym --release "Super_L" mode $hov $border, $focus_one, fullscreen disable; $exec $touchpad_off; $exec $alert $hov & $oklò_enable

 # kill
 bindsym Mod1+F4 kill; $refresh_status_bar

 # borders
 bindsym Mod4+b $border
 bindsym Mod4+Shift+b $no_border

 # browse workspaces
 bindsym Mod4+j workspace prev
 bindsym Mod4+agrave workspace next
 bindsym Mod4+ugrave workspace back_and_forth

 # default move
 bindsym Mod4+Shift+Up move up
 bindsym Mod4+Shift+Left move left
 bindsym Mod4+Shift+Down move down
 bindsym Mod4+Shift+Right move right

 # focus group
 bindsym Mod4+less focus child
 bindsym Mod4+Shift+greater focus parent

 # fullscreen
 bindsym Mod4+f fullscreen toggle
 bindsym Mod4+Shift+f $exec "sleep 0.5; xdotool key F11; sleep 0.5; i3-msg fullscreen disable"

 # oklò move
 bindsym Mod4+Shift+o move up
 bindsym Mod4+Shift+k move left
 bindsym Mod4+Shift+l move down
 bindsym Mod4+Shift+ograve move right

 # open emacs
 bindsym Mod4+e exec $emacs

 # open terminal
 bindsym Mod4+Return exec gnome-terminal

 # resize window
 bindsym Mod4+i resize shrink height 5 px or 5 ppt
 bindsym Mod4+p resize  grow  height 5 px or 5 ppt
 bindsym Mod4+Prior resize shrink height 5 px or 5 ppt
 bindsym Mod4+Next resize  grow  height 5 px or 5 ppt
 bindsym Mod4+Shift+i resize shrink width  5 px or 5 ppt
 bindsym Mod4+Shift+p resize  grow  width  5 px or 5 ppt
 bindsym Mod4+Shift+Prior resize shrink width  5 px or 5 ppt
 bindsym Mod4+Shift+Next resize  grow  width  5 px or 5 ppt

 # split orientation
 bindsym Mod4+v split v; exec notify-send 'vertical'
 bindsym Mod4+h split h; exec notify-send 'horizontal'
 bindsym Mod4+g split v; focus parent; layout toggle split; focus child

 # wasd move
 bindsym Mod4+Shift+w move up
 bindsym Mod4+Shift+a move left
 bindsym Mod4+Shift+s move down
 bindsym Mod4+Shift+d move right

 # files
 bindsym Mod4+control+f $exec $fm
 bindsym Mod4+control+j $exec $fm Downloads

 # focus all
 bindsym Mod4+control+a $focus_all

 # move focused container to workspace and follow
 bindsym Mod4+control+1 move container to workspace $w1; workspace $w1
 bindsym Mod4+control+2 move container to workspace $w2; workspace $w2
 bindsym Mod4+control+3 move container to workspace $w3; workspace $w3
 bindsym Mod4+control+4 move container to workspace $w4; workspace $w4
 bindsym Mod4+control+5 move container to workspace $w5; workspace $w5
 bindsym Mod4+control+6 move container to workspace $w6; workspace $w6
 bindsym Mod4+control+7 move container to workspace $w7; workspace $w7
 bindsym Mod4+control+8 move container to workspace $w8; workspace $w8
 bindsym Mod4+control+9 move container to workspace $w9; workspace $w9

 # reload configuration
 bindsym Mod4+control+r $exec "make -C ~/.i3 && i3-msg $no_border restart"
 bindsym Mod4+control+Shift+r $exec "make -C ~/.i3 stable && i3-msg $no_border restart"

 # resize (precise)
 bindsym Mod4+control+i resize shrink height 1 px or 1 ppt
 bindsym Mod4+control+p resize  grow  height 1 px or 1 ppt
 bindsym Mod4+control+Prior resize shrink height 1 px or 1 ppt
 bindsym Mod4+control+Next resize  grow  height 1 px or 1 ppt
 bindsym Mod4+control+Shift+i resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+p resize  grow  width  1 px or 1 ppt
 bindsym Mod4+control+Shift+Prior resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+Next resize  grow  width  1 px or 1 ppt

 # resize (scale)
 bindsym Mod4+control+minus resize shrink height 5 px or 5 ppt; resize shrink width  5 px or 5 ppt
 bindsym Mod4+control+plus resize  grow  height 5 px or 5 ppt; resize  grow  width  5 px or 5 ppt
 bindsym Mod4+control+Shift+minus resize shrink height 1 px or 1 ppt; resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+plus resize  grow  height 1 px or 1 ppt; resize  grow  width  1 px or 1 ppt

 # save layout
 bindsym Mod4+control+s exec "i3-save-tree > ~/.workspaces/stamp.json; $emacs ~/.workspaces/stamp.json"

 # start dmenu for applications
 bindsym Mod4+Shift+exclam mode $def $exec $dmenu; $exec $alert $wrt

 # start dmenu for commands
 bindsym Mod4+control+e mode $def $exec dmenu_run; $exec $alert $wrt

 # application menu
 bindsym Mod4+Menu mode $hov exec morc_menu; $exec $alert $hov & $oklò_enable

 # no border touch
 bindsym Mod4+Shift+space mode $tch $no_border; $exec $alert $tch & $touchpad_on

 # browse workspaces
 bindsym Mod4+Tab mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Shift+Tab mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # default focus
 bindsym Mod4+Up mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Left mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Down mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Right mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # oklò focus
 bindsym Mod4+o mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+k mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+l mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+ograve mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # switch workspace
 bindsym Mod4+1 mode $hov workspace $w1; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+2 mode $hov workspace $w2; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+3 mode $hov workspace $w3; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+4 mode $hov workspace $w4; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+5 mode $hov workspace $w5; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+6 mode $hov workspace $w6; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+7 mode $hov workspace $w7; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+8 mode $hov workspace $w8; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+9 mode $hov workspace $w9; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # wasd focus
 bindsym Mod4+w mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+a mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+s mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+d mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # cut
 bindsym Mod4+control+x mode $hov move container to workspace $wx; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # kill
 bindsym Mod4+control+w mode $hov kill; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # paste
 bindsym Mod4+control+v mode $hov $exec "i3-msg 'workspace --no-auto-back-and-forth $wx; move container to workspace $wx; workspace $wx'"; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # unused keys
 bindsym backslash nop
 bindsym 1 nop
 bindsym 2 nop
 bindsym 3 nop
 bindsym 4 nop
 bindsym 5 nop
 bindsym 6 nop
 bindsym 7 nop
 bindsym 8 nop
 bindsym 9 nop
 bindsym 0 nop
 bindsym apostrophe nop
 bindsym igrave nop
 bindsym Tab nop
 bindsym q nop
 bindsym w nop
 bindsym e nop
 bindsym r nop
 bindsym t nop
 bindsym y nop
 bindsym u nop
 bindsym i nop
 bindsym o nop
 bindsym p nop
 bindsym egrave nop
 bindsym plus nop
 bindsym "Caps_Lock" nop
 bindsym a nop
 bindsym s nop
 bindsym d nop
 bindsym f nop
 bindsym g nop
 bindsym h nop
 bindsym j nop
 bindsym k nop
 bindsym l nop
 bindsym ograve nop
 bindsym agrave nop
 bindsym ugrave nop
 bindsym "Shift_L" nop
 bindsym less nop
 bindsym z nop
 bindsym x nop
 bindsym c nop
 bindsym v nop
 bindsym b nop
 bindsym n nop
 bindsym m nop
 bindsym comma nop
 bindsym period nop
 bindsym minus nop
 bindsym "Shift_R" nop
 bindsym "Control_L" nop
 bindsym "Super_L" nop
 bindsym $alt_gr nop
 bindsym "Control_R" nop
 bindsym Up nop
 bindsym Left nop
 bindsym Down nop
 bindsym Right nop

 }

