 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                               SETTINGS                                      #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 
 # Set mod key (Mod1=<Alt>, Mod4=<Super>)
 set $mod Mod4
 
 # set default desktop layout (default is tiling)
 # workspace_layout tabbed <stacking|tabbed>
 
 # Configure border style <normal|1pixel|pixel xx|none|pixel>
 new_window pixel 0
 new_float normal
 
 # Hide borders
 hide_edge_borders none
 
 # Font for window titles. Will also be used by the bar unless a different font
 # is used in the bar {} block below.
 font pango:DejaVu Sans Mono 8

 # go back to previous workspace if you try to go to the current one
 # (allow for fast window movement to a workspace and back)
 workspace_auto_back_and_forth yes

 # focus on mouseover (is a pain if you happen to have a mouse)
 focus_follows_mouse no

 # I like the original snake better
 focus_wrapping no

 # smart|ignore|leave_fullscreen
 popup_during_fullscreen leave_fullscreen

 # smart|urgent|focus|none
 focus_on_window_activation none

 #                                 COLORS                                      #

 # class                 border  backgr. text    ind.    child
 client.focused          #4CC97E #4CC97E #ffffff #4CC97E #4CC97E
 client.focused_inactive #222222 #222222 #ffffff #222222 #222222
 client.unfocused        #222222 #222222 #888888 #222222 #222222
 client.urgent           #222222 #900000 #ffffff #900000 #900000
 client.placeholder      #000000 #222222 #ffffff #000000 #222222

 client.background       #222222

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                               STATUS-BAR                                    #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 bar {
        status_command    ./.bin/my_status.sh
        position          bottom
        mode              dock
        workspace_buttons yes
        colors {
                   background #000000
                   statusline #999999
                   focused_workspace  #ffffff #000000
                   active_workspace   #ffffff #000000
                   inactive_workspace #888888 #000000
                   urgent_workspace   #ffffff #000000
               }

     # disable scrolling on status bar
        bindsym button4 nop # scroll wheel up
        bindsym button5 nop # scroll wheel down

     # no border on middle mouse
        bindsym button2 $no_border
 }

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                               DEFINITIONS                                   #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 # The (new) i3-dmenu-desktop which only displays applications
 # shipping a .desktop file. It is a wrapper around dmenu, so you need that
 # installed.
 set $dmenu i3-dmenu-desktop

 # file manager
 set $fm nautilus -w

 # for brevity
 set $exec exec --no-startup-id
 set $exec_always exec_always --no-startup-id
 set $alert notify-send --expire-time=1200
 set $refresh_status_bar exec killall -USR1 i3status
 set $focus_all focus parent; focus parent; focus parent; focus parent
 set $focus_one focus child ; focus child ; focus child ; focus child
 set $no_border [tiling] border none
 set $border [tiling] border pixel 5
 set $alt_gr "ISO_Level3_Shift"
 set $emacs emacsclient -create-frame --alternate-editor=""

 #                                KEYBINDINGS                                  #

 set $disable_Caps setxkbmap -option ctrl:nocaps
 set $enable_Caps setxkbmap -option
 set $oklò_disable ~/.bin/oklò.sh 0
 set $oklò_enable ~/.bin/oklò.sh 1

 #                              FUNCTIONALITIES                                #

 # touchpad toggle
 set $touchpad_on ~/.bin/touchpad_toggle.sh 1 & xdotool mousemove 800 450
 set $touchpad_off ~/.bin/touchpad_toggle.sh 0 & xdotool mousemove 2500 1200
 set $touchpad_off_2 ~/.bin/touchpad_toggle.sh 0
 set $touchpad_toggle ~/.bin/touchpad_toggle.sh

 # autoscroll
 set $disable_autoscroll ~/.bin/autoscroll.sh 0
 set $enable_autoscroll ~/.bin/autoscroll.sh 1

 # brightness
 set $brightness_up exec "xbacklight -inc 5; notify-send 'brightness up'"
 set $brightness_down exec "xbacklight -dec 5; notify-send 'brightness down'"

 #                                 WORKSPACES                                  #

 set $w1 1nternet
 set $w2 2rectory
 set $w3 3code
 set $w4 4terminl
 set $w5 5team
 set $w6 6iscord
 set $w7 7atex
 set $w8 8volante
 set $w9 9imp


 # this one is for cut and paste
 set $wx X


 #                                  LAYOUTS                                    #

 set $layout_1 i3-msg 'workspace --no-auto-back-and-forth 1nternet; split h; focus parent; focus parent; focus parent; focus parent; kill; append_layout ~/.workspaces/1nternet.json'
 set $layout_2 i3-msg 'workspace --no-auto-back-and-forth 2rectory; split h; focus parent; focus parent; focus parent; focus parent; focus parent; kill; append_layout ~/.workspaces/2rectory.json'
 set $layout_3 i3-msg 'workspace --no-auto-back-and-forth 3code; split h; focus parent; focus parent; focus parent; focus parent; focus parent; kill'

 set $fill_1 chromium --new-window --profile-directory=Default https://web.telegram.org/ https://web.whatsapp.com/ & sleep 0.25; chromium --new-window --profile-directory=Default https://mail.google.com/mail/u/0/ https://mail.google.com/mail/u/1/ https://calendar.google.com/calendar/ & sleep 0.5; chromium --new-window --profile-directory=Default https://keep.google.com/u/0/ https://getpocket.com/a/queue/list/ & sleep 2; i3-msg [class='.?'] border none
 set $fill_2 nautilus -w & nautilus -w Downloads & emacsclient -create-frame --alternate-editor='' ~/memo & sleep 0.5; chromium --new-window --profile-directory=Default https://github.com/kappanneo https://gitlab.com/kappanneo & gnome-terminal & sleep 2; i3-msg [class='.?'] border none
 set $fill_3 code-oss & sleep 2; i3-msg [class='.?'] border none
 #                                    MODES                                    #

 set $def "default"
 set $cnf "CONFG: [c]onfigure i3  .git[i]gnore  [a]pplications  [e]macs  [s]tatusbar  [z]sh  [g]uide  [1|2|l]ayouts  [r]edshift  [space|esc] exit mode"
 set $hov "HOVER: writing disabled  [oklò] move cursor  [0] insert  [space|esc] write mode"
 set $pow "POWER: [q]uit  [r]estart  [s]uspend  [h]ibernate  [esc]"
 set $red "REDSH: [123] shift red level  [+] increase  [space|esc] exit mode"
 set $str "START: [1|2] layouts  [spaec|esc] exit mode"
 set $tch "TOUCH: touchpad enabled  [space|esc] write mode"
 set $wrt "WRITE: writing enabled"


 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                APPLICATIONS                                 #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 # code
 assign [class=".*code.*"] $w3

 # android studio
 assign [class="jetbrains-studio"] $w3

 # steam
 assign [class="Steam"] $w5
 assign [class="Wine"] $w5
 for_window [title="Steam - News"] kill

 # discord and skype
 assign [class="discord"] $w6
 assign [title="Telegram"] $w6
 assign [title="Skype"] $w6

 # texmaker
 assign [title="Texmaker"] $w7
 assign [title=".*overleaf.*"] $w7

 # gimp
 assign [class="Gimp.*"] $w9

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                ON-NEW-WINDOW                                #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 for_window [class=".?" title=".?"] split toggle, fullscreen disable, $border

 for_window [floating] floating disable

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                  STARTUP                                    #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 # keybinding
 $exec $disable_Caps

 # start with touchpad disabled
 $exec $touchpad_off
 $exec $enable_autoscroll

 # background (not always permanent, may need reaload)
 $exec_always $enable_autoscroll
 $exec_always nitrogen --restore
 $exec_always fix_xcursor

 # background applications
 $exec /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1
 $exec nitrogen --restore; sleep 1; compton -b
 $exec xfce4-power-manager
 # $exec start_conky_maja
 $exec alttab -s 1 -d 1
 
 # bar applications
 $exec nm-applet
 $exec pamac-tray
 $exec pa-applet
 $exec clipit

 # reshift oneshot mode (to set starting temperature according to time)
 $exec redshift -o

 # premade layout
 $exec $layout_2; $fill_2

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                  COMMANDS                                   #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 # borders
 bindsym --border button2 $no_border; $exec echo --border button2 >> log

 # display
 bindsym XF86Display $exec xrandr --output eDP1 --mode 1920x1080 --preferred; $exec echo XF86Display >> log

 # screen brightness controls
 bindsym XF86MonBrightnessUp $brightness_up; $exec echo XF86MonBrightnessUp >> log
 bindsym XF86MonBrightnessDown $brightness_down; $exec echo XF86MonBrightnessDown >> log

 # screen poweroff
 bindsym XF86ScreenSaver $exec xset dpms force off; $exec echo XF86ScreenSaver >> log

 # screenshot
 bindsym --release Print $exec i3-scrot; $exec echo --release Print >> log
 bindsym --release Mod4+Print $exec i3-scrot -w; $exec echo --release Mod4+Print >> log
 bindsym --release Mod4+Shift+Print $exec i3-scrot -s; $exec echo --release Mod4+Shift+Print >> log

 # touchpad toggle
 bindsym XF86TouchpadToggle $exec $touchpad_toggle; $exec echo XF86TouchpadToggle >> log

 # no border write
 bindsym Shift+"Super_L" $no_border; $exec echo Shift+"Super_L" >> log

 # no border hover
 bindsym Shift+Menu mode $hov $no_border; $exec $alert $hov & $oklò_enable; $exec echo Shift+Menu >> log

 # where am i?
 bindsym --release "Super_L" mode $hov $border, $focus_one, fullscreen disable; $exec $touchpad_off; $exec $alert $hov & $oklò_enable; $exec echo --release "Super_L" >> log



 #                                ALT-COMMANDS                                 #

 # kill
 bindsym Mod1+F4 kill; $refresh_status_bar; $exec echo Mod1+F4 >> log



 #                               SUPER-COMMANDS                                #

 # borders
 bindsym Mod4+b $border; $exec echo Mod4+b >> log
 bindsym Mod4+Shift+b $no_border; $exec echo Mod4+Shift+b >> log

 # default move
 bindsym Mod4+Shift+Up move up; $exec echo Mod4+Shift+Up >> log
 bindsym Mod4+Shift+Left move left; $exec echo Mod4+Shift+Left >> log
 bindsym Mod4+Shift+Down move down; $exec echo Mod4+Shift+Down >> log
 bindsym Mod4+Shift+Right move right; $exec echo Mod4+Shift+Right >> log

 # focus group
 bindsym Mod4+less focus child; $exec echo Mod4+less >> log
 bindsym Mod4+Shift+greater focus parent; $exec echo Mod4+Shift+greater >> log

 # fullscreen
 bindsym Mod4+f fullscreen toggle; $exec echo Mod4+f >> log
 bindsym Mod4+Shift+f $exec "sleep 0.5; xdotool key F11; sleep 0.25; i3-msg fullscreen disable"; $exec echo Mod4+Shift+f >> log

 # oklò move
 bindsym Mod4+Shift+o move up; $exec echo Mod4+Shift+o >> log
 bindsym Mod4+Shift+k move left; $exec echo Mod4+Shift+k >> log
 bindsym Mod4+Shift+l move down; $exec echo Mod4+Shift+l >> log
 bindsym Mod4+Shift+ograve move right; $exec echo Mod4+Shift+ograve >> log

 # open emacs
 bindsym Mod4+e exec $emacs; $exec echo Mod4+e >> log

 # open terminal
 bindsym Mod4+Return exec gnome-terminal; $exec echo Mod4+Return >> log

 # resize window
 bindsym Mod4+i resize shrink height 5 px or 5 ppt; $exec echo Mod4+i >> log
 bindsym Mod4+p resize  grow  height 5 px or 5 ppt; $exec echo Mod4+p >> log
 bindsym Mod4+Prior resize shrink height 5 px or 5 ppt; $exec echo Mod4+Prior >> log
 bindsym Mod4+Next resize  grow  height 5 px or 5 ppt; $exec echo Mod4+Next >> log
 bindsym Mod4+Shift+i resize shrink width  5 px or 5 ppt; $exec echo Mod4+Shift+i >> log
 bindsym Mod4+Shift+p resize  grow  width  5 px or 5 ppt; $exec echo Mod4+Shift+p >> log
 bindsym Mod4+Shift+Prior resize shrink width  5 px or 5 ppt; $exec echo Mod4+Shift+Prior >> log
 bindsym Mod4+Shift+Next resize  grow  width  5 px or 5 ppt; $exec echo Mod4+Shift+Next >> log

 # split orientation
 bindsym Mod4+v split v; exec notify-send 'vertical'; $exec echo Mod4+v >> log
 bindsym Mod4+h split h; exec notify-send 'horizontal'; $exec echo Mod4+h >> log
 bindsym Mod4+g split v; focus parent; layout toggle split; focus child; $exec echo Mod4+g >> log

 # wasd move
 bindsym Mod4+Shift+w move up; $exec echo Mod4+Shift+w >> log
 bindsym Mod4+Shift+a move left; $exec echo Mod4+Shift+a >> log
 bindsym Mod4+Shift+s move down; $exec echo Mod4+Shift+s >> log
 bindsym Mod4+Shift+d move right; $exec echo Mod4+Shift+d >> log

 # default split and move
 bindsym Mod4+control+Shift+Up focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+Up >> log
 bindsym Mod4+control+Shift+Left focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+Left >> log
 bindsym Mod4+control+Shift+Down focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+Down >> log
 bindsym Mod4+control+Shift+Right focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+Right >> log

 # files
 bindsym Mod4+control+f $exec $fm; $exec echo Mod4+control+f >> log
 bindsym Mod4+control+j $exec $fm Downloads; $exec echo Mod4+control+j >> log

 # focus all
 bindsym Mod4+control+a $focus_all; $exec echo Mod4+control+a >> log

 # move container while browsing workspaces
 bindsym Mod4+control+Tab move container to workspace next; workspace next; $exec echo Mod4+control+Tab >> log
 bindsym Mod4+control+Shift+Tab move container to workspace prev; workspace prev; $exec echo Mod4+control+Shift+Tab >> log
 bindsym Mod4+control+Home move container to workspace prev; workspace prev; $exec echo Mod4+control+Home >> log
 bindsym Mod4+control+End move container to workspace next; workspace next; $exec echo Mod4+control+End >> log
 bindsym Mod4+control+ugrave move container to workspace back_and_forth; workspace back_and_forth; $exec echo Mod4+control+ugrave >> log

 # move focused container to workspace and follow
 bindsym Mod4+control+1 move container to workspace $w1; workspace $w1; $exec echo Mod4+control+1 >> log
 bindsym Mod4+control+2 move container to workspace $w2; workspace $w2; $exec echo Mod4+control+2 >> log
 bindsym Mod4+control+3 move container to workspace $w3; workspace $w3; $exec echo Mod4+control+3 >> log
 bindsym Mod4+control+4 move container to workspace $w4; workspace $w4; $exec echo Mod4+control+4 >> log
 bindsym Mod4+control+5 move container to workspace $w5; workspace $w5; $exec echo Mod4+control+5 >> log
 bindsym Mod4+control+6 move container to workspace $w6; workspace $w6; $exec echo Mod4+control+6 >> log
 bindsym Mod4+control+7 move container to workspace $w7; workspace $w7; $exec echo Mod4+control+7 >> log
 bindsym Mod4+control+8 move container to workspace $w8; workspace $w8; $exec echo Mod4+control+8 >> log
 bindsym Mod4+control+9 move container to workspace $w9; workspace $w9; $exec echo Mod4+control+9 >> log

 # oklò split and move
 bindsym Mod4+control+Shift+o focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+o >> log
 bindsym Mod4+control+Shift+k focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+k >> log
 bindsym Mod4+control+Shift+l focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+l >> log
 bindsym Mod4+control+Shift+ograve focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+ograve >> log

 # reload configuration
 bindsym Mod4+control+r $exec "make -C ~/.i3 && i3-msg $no_border restart"; $exec echo Mod4+control+r >> log
 bindsym Mod4+control+Shift+r $exec "make -C ~/.i3 stable && i3-msg $no_border restart"; $exec echo Mod4+control+Shift+r >> log

 # resize (precise)
 bindsym Mod4+control+i resize shrink height 1 px or 1 ppt; $exec echo Mod4+control+i >> log
 bindsym Mod4+control+p resize  grow  height 1 px or 1 ppt; $exec echo Mod4+control+p >> log
 bindsym Mod4+control+Prior resize shrink height 1 px or 1 ppt; $exec echo Mod4+control+Prior >> log
 bindsym Mod4+control+Next resize  grow  height 1 px or 1 ppt; $exec echo Mod4+control+Next >> log
 bindsym Mod4+control+Shift+i resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+i >> log
 bindsym Mod4+control+Shift+p resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+p >> log
 bindsym Mod4+control+Shift+Prior resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+Prior >> log
 bindsym Mod4+control+Shift+Next resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+Next >> log

 # resize (scale)
 bindsym Mod4+control+minus resize shrink height 5 px or 5 ppt; resize shrink width  5 px or 5 ppt; $exec echo Mod4+control+minus >> log
 bindsym Mod4+control+plus resize  grow  height 5 px or 5 ppt; resize  grow  width  5 px or 5 ppt; $exec echo Mod4+control+plus >> log
 bindsym Mod4+control+Shift+minus resize shrink height 1 px or 1 ppt; resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+minus >> log
 bindsym Mod4+control+Shift+plus resize  grow  height 1 px or 1 ppt; resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+plus >> log

 # save layout
 bindsym Mod4+control+s exec "i3-save-tree > ~/.workspaces/stamp.json; emacsclient -create-frame --alternate-editor='' ~/.workspaces/stamp.json"; $exec echo Mod4+control+s >> log

 # wasd split and move
 bindsym Mod4+control+Shift+w focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+w >> log
 bindsym Mod4+control+Shift+a focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+a >> log
 bindsym Mod4+control+Shift+s focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+s >> log
 bindsym Mod4+control+Shift+d focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+d >> log

 # f-fake fullscren
 bindsym Mod4+control+Shift+f $exec "sleep 0.5; xdotool key f; sleep 0.25; i3-msg fullscreen disable"; $exec echo Mod4+control+Shift+f >> log

 # start dmenu for applications
 bindsym Mod4+Shift+exclam $exec $dmenu; $exec echo Mod4+Shift+exclam >> log

 # start dmenu for commands
 bindsym Mod4+control+e $exec dmenu_run; $exec echo Mod4+control+e >> log

 # application menu
 bindsym Mod4+Menu mode $hov exec morc_menu; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Menu >> log

 # no border touch
 bindsym Mod4+Shift+space mode $tch $no_border; $exec $alert $tch & $touchpad_on; $exec echo Mod4+Shift+space >> log

 # browse workspaces
 bindsym Mod4+Tab mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Tab >> log
 bindsym Mod4+Shift+Tab mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Shift+Tab >> log
 bindsym Mod4+j mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+j >> log
 bindsym Mod4+agrave mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+agrave >> log
 bindsym Mod4+Home mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Home >> log
 bindsym Mod4+End mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+End >> log
 bindsym Mod4+ugrave mode $hov workspace back_and_forth; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+ugrave >> log

 # default focus
 bindsym Mod4+Up mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Up >> log
 bindsym Mod4+Left mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Left >> log
 bindsym Mod4+Down mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Down >> log
 bindsym Mod4+Right mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Right >> log

 # oklò focus
 bindsym Mod4+o mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+o >> log
 bindsym Mod4+k mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+k >> log
 bindsym Mod4+l mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+l >> log
 bindsym Mod4+ograve mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+ograve >> log

 # switch workspace
 bindsym Mod4+1 mode $hov workspace $w1; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+1 >> log
 bindsym Mod4+2 mode $hov workspace $w2; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+2 >> log
 bindsym Mod4+3 mode $hov workspace $w3; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+3 >> log
 bindsym Mod4+4 mode $hov workspace $w4; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+4 >> log
 bindsym Mod4+5 mode $hov workspace $w5; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+5 >> log
 bindsym Mod4+6 mode $hov workspace $w6; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+6 >> log
 bindsym Mod4+7 mode $hov workspace $w7; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+7 >> log
 bindsym Mod4+8 mode $hov workspace $w8; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+8 >> log
 bindsym Mod4+9 mode $hov workspace $w9; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+9 >> log

 # wasd focus
 bindsym Mod4+w mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+w >> log
 bindsym Mod4+a mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+a >> log
 bindsym Mod4+s mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+s >> log
 bindsym Mod4+d mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+d >> log

 # cut
 bindsym Mod4+control+x mode $hov move container to workspace $wx; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+control+x >> log

 # kill
 bindsym Mod4+control+w mode $hov kill; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+control+w >> log

 # paste
 bindsym Mod4+control+v mode $hov $exec "i3-msg 'workspace --no-auto-back-and-forth $wx; move container to workspace $wx; workspace $wx'"; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+control+v >> log



 #                                   MODES                                     #

 bindsym Mod4+c mode $cnf $exec $alert $cnf; $exec echo Mod4+c >> log

 bindsym Mod1+Menu mode $hov $exec $alert $hov & $oklò_enable; $exec echo Mod1+Menu >> log
 bindsym Menu mode $hov $exec $alert $hov & $oklò_enable; $exec echo Menu >> log

 bindsym XF86PowerOff mode $pow $exec $alert $pow, fullscreen disable; $exec echo XF86PowerOff >> log

 bindsym Mod4+r mode $red $exec $alert $red; $exec echo Mod4+r >> log

 bindsym Mod4+z mode $str $exec $alert $str; $exec echo Mod4+z >> log

 bindsym Mod4+space mode $tch $exec $alert $tch & $touchpad_on; $exec echo Mod4+space >> log



 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                   MODES                                     #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 #                               POWEROFF-MODE                                 #

 mode $pow {

 # options
 bindsym XF86PowerOff mode $def $exec $alert $pow; $exec $alert $wrt; $exec echo XF86PowerOff >> log
 bindsym q mode $def $exec systemctl poweroff; $exec $alert $wrt; $exec echo q >> log
 bindsym r mode $def $exec systemctl reboot; $exec $alert $wrt; $exec echo r >> log
 bindsym s mode $def $exec systemctl suspend; $exec $alert $wrt; $exec echo s >> log
 bindsym h mode $def $exec systemctl hibernate; $exec $alert $wrt; $exec echo h >> log

 bindsym Mod4+c mode $cnf $exec $alert $cnf; $exec echo Mod4+c >> log

 bindsym Mod1+Menu mode $hov $exec $alert $hov & $oklò_enable; $exec echo Mod1+Menu >> log
 bindsym Menu mode $hov $exec $alert $hov & $oklò_enable; $exec echo Menu >> log

 bindsym Mod4+r mode $red $exec $alert $red; $exec echo Mod4+r >> log

 bindsym Mod4+z mode $str $exec $alert $str; $exec echo Mod4+z >> log

 bindsym Mod4+space mode $tch $exec $alert $tch & $touchpad_on; $exec echo Mod4+space >> log

 bindsym Return mode $def $exec $alert $wrt; $exec echo Return >> log
 bindsym BackSpace mode $def $exec $alert $wrt; $exec echo BackSpace >> log
 bindsym space mode $def $exec $alert $wrt; $exec echo space >> log
 bindsym Delete mode $def $exec $alert $wrt; $exec echo Delete >> log
 bindsym "Alt_L" mode $def $exec $alert $wrt; $exec echo "Alt_L" >> log

 # borders
 bindsym --border button2 $no_border; $exec echo --border button2 >> log

 # display
 bindsym XF86Display $exec xrandr --output eDP1 --mode 1920x1080 --preferred; $exec echo XF86Display >> log

 # screen brightness controls
 bindsym XF86MonBrightnessUp $brightness_up; $exec echo XF86MonBrightnessUp >> log
 bindsym XF86MonBrightnessDown $brightness_down; $exec echo XF86MonBrightnessDown >> log

 # screen poweroff
 bindsym XF86ScreenSaver $exec xset dpms force off; $exec echo XF86ScreenSaver >> log

 # screenshot
 bindsym --release Print $exec i3-scrot; $exec echo --release Print >> log
 bindsym --release Mod4+Print $exec i3-scrot -w; $exec echo --release Mod4+Print >> log
 bindsym --release Mod4+Shift+Print $exec i3-scrot -s; $exec echo --release Mod4+Shift+Print >> log

 # touchpad toggle
 bindsym XF86TouchpadToggle $exec $touchpad_toggle; $exec echo XF86TouchpadToggle >> log

 # no border write
 bindsym Shift+"Super_L" mode $def $no_border; $exec $alert $wrt; $exec echo Shift+"Super_L" >> log

 # no border hover
 bindsym Shift+Menu mode $hov $no_border; $exec $alert $hov & $oklò_enable; $exec echo Shift+Menu >> log

 # where am i?
 bindsym --release "Super_L" mode $hov $border, $focus_one, fullscreen disable; $exec $touchpad_off; $exec $alert $hov & $oklò_enable; $exec echo --release "Super_L" >> log

 # kill
 bindsym Mod1+F4 kill; $refresh_status_bar; $exec echo Mod1+F4 >> log

 # borders
 bindsym Mod4+b $border; $exec echo Mod4+b >> log
 bindsym Mod4+Shift+b $no_border; $exec echo Mod4+Shift+b >> log

 # default move
 bindsym Mod4+Shift+Up move up; $exec echo Mod4+Shift+Up >> log
 bindsym Mod4+Shift+Left move left; $exec echo Mod4+Shift+Left >> log
 bindsym Mod4+Shift+Down move down; $exec echo Mod4+Shift+Down >> log
 bindsym Mod4+Shift+Right move right; $exec echo Mod4+Shift+Right >> log

 # focus group
 bindsym Mod4+less focus child; $exec echo Mod4+less >> log
 bindsym Mod4+Shift+greater focus parent; $exec echo Mod4+Shift+greater >> log

 # fullscreen
 bindsym Mod4+f fullscreen toggle; $exec echo Mod4+f >> log
 bindsym Mod4+Shift+f $exec "sleep 0.5; xdotool key F11; sleep 0.25; i3-msg fullscreen disable"; $exec echo Mod4+Shift+f >> log

 # oklò move
 bindsym Mod4+Shift+o move up; $exec echo Mod4+Shift+o >> log
 bindsym Mod4+Shift+k move left; $exec echo Mod4+Shift+k >> log
 bindsym Mod4+Shift+l move down; $exec echo Mod4+Shift+l >> log
 bindsym Mod4+Shift+ograve move right; $exec echo Mod4+Shift+ograve >> log

 # open emacs
 bindsym Mod4+e exec $emacs; $exec echo Mod4+e >> log

 # open terminal
 bindsym Mod4+Return exec gnome-terminal; $exec echo Mod4+Return >> log

 # resize window
 bindsym Mod4+i resize shrink height 5 px or 5 ppt; $exec echo Mod4+i >> log
 bindsym Mod4+p resize  grow  height 5 px or 5 ppt; $exec echo Mod4+p >> log
 bindsym Mod4+Prior resize shrink height 5 px or 5 ppt; $exec echo Mod4+Prior >> log
 bindsym Mod4+Next resize  grow  height 5 px or 5 ppt; $exec echo Mod4+Next >> log
 bindsym Mod4+Shift+i resize shrink width  5 px or 5 ppt; $exec echo Mod4+Shift+i >> log
 bindsym Mod4+Shift+p resize  grow  width  5 px or 5 ppt; $exec echo Mod4+Shift+p >> log
 bindsym Mod4+Shift+Prior resize shrink width  5 px or 5 ppt; $exec echo Mod4+Shift+Prior >> log
 bindsym Mod4+Shift+Next resize  grow  width  5 px or 5 ppt; $exec echo Mod4+Shift+Next >> log

 # split orientation
 bindsym Mod4+v split v; exec notify-send 'vertical'; $exec echo Mod4+v >> log
 bindsym Mod4+h split h; exec notify-send 'horizontal'; $exec echo Mod4+h >> log
 bindsym Mod4+g split v; focus parent; layout toggle split; focus child; $exec echo Mod4+g >> log

 # wasd move
 bindsym Mod4+Shift+w move up; $exec echo Mod4+Shift+w >> log
 bindsym Mod4+Shift+a move left; $exec echo Mod4+Shift+a >> log
 bindsym Mod4+Shift+s move down; $exec echo Mod4+Shift+s >> log
 bindsym Mod4+Shift+d move right; $exec echo Mod4+Shift+d >> log

 # default split and move
 bindsym Mod4+control+Shift+Up focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+Up >> log
 bindsym Mod4+control+Shift+Left focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+Left >> log
 bindsym Mod4+control+Shift+Down focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+Down >> log
 bindsym Mod4+control+Shift+Right focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+Right >> log

 # files
 bindsym Mod4+control+f $exec $fm; $exec echo Mod4+control+f >> log
 bindsym Mod4+control+j $exec $fm Downloads; $exec echo Mod4+control+j >> log

 # focus all
 bindsym Mod4+control+a $focus_all; $exec echo Mod4+control+a >> log

 # move container while browsing workspaces
 bindsym Mod4+control+Tab move container to workspace next; workspace next; $exec echo Mod4+control+Tab >> log
 bindsym Mod4+control+Shift+Tab move container to workspace prev; workspace prev; $exec echo Mod4+control+Shift+Tab >> log
 bindsym Mod4+control+Home move container to workspace prev; workspace prev; $exec echo Mod4+control+Home >> log
 bindsym Mod4+control+End move container to workspace next; workspace next; $exec echo Mod4+control+End >> log
 bindsym Mod4+control+ugrave move container to workspace back_and_forth; workspace back_and_forth; $exec echo Mod4+control+ugrave >> log

 # move focused container to workspace and follow
 bindsym Mod4+control+1 move container to workspace $w1; workspace $w1; $exec echo Mod4+control+1 >> log
 bindsym Mod4+control+2 move container to workspace $w2; workspace $w2; $exec echo Mod4+control+2 >> log
 bindsym Mod4+control+3 move container to workspace $w3; workspace $w3; $exec echo Mod4+control+3 >> log
 bindsym Mod4+control+4 move container to workspace $w4; workspace $w4; $exec echo Mod4+control+4 >> log
 bindsym Mod4+control+5 move container to workspace $w5; workspace $w5; $exec echo Mod4+control+5 >> log
 bindsym Mod4+control+6 move container to workspace $w6; workspace $w6; $exec echo Mod4+control+6 >> log
 bindsym Mod4+control+7 move container to workspace $w7; workspace $w7; $exec echo Mod4+control+7 >> log
 bindsym Mod4+control+8 move container to workspace $w8; workspace $w8; $exec echo Mod4+control+8 >> log
 bindsym Mod4+control+9 move container to workspace $w9; workspace $w9; $exec echo Mod4+control+9 >> log

 # oklò split and move
 bindsym Mod4+control+Shift+o focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+o >> log
 bindsym Mod4+control+Shift+k focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+k >> log
 bindsym Mod4+control+Shift+l focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+l >> log
 bindsym Mod4+control+Shift+ograve focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+ograve >> log

 # reload configuration
 bindsym Mod4+control+r $exec "make -C ~/.i3 && i3-msg $no_border restart"; $exec echo Mod4+control+r >> log
 bindsym Mod4+control+Shift+r $exec "make -C ~/.i3 stable && i3-msg $no_border restart"; $exec echo Mod4+control+Shift+r >> log

 # resize (precise)
 bindsym Mod4+control+i resize shrink height 1 px or 1 ppt; $exec echo Mod4+control+i >> log
 bindsym Mod4+control+p resize  grow  height 1 px or 1 ppt; $exec echo Mod4+control+p >> log
 bindsym Mod4+control+Prior resize shrink height 1 px or 1 ppt; $exec echo Mod4+control+Prior >> log
 bindsym Mod4+control+Next resize  grow  height 1 px or 1 ppt; $exec echo Mod4+control+Next >> log
 bindsym Mod4+control+Shift+i resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+i >> log
 bindsym Mod4+control+Shift+p resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+p >> log
 bindsym Mod4+control+Shift+Prior resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+Prior >> log
 bindsym Mod4+control+Shift+Next resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+Next >> log

 # resize (scale)
 bindsym Mod4+control+minus resize shrink height 5 px or 5 ppt; resize shrink width  5 px or 5 ppt; $exec echo Mod4+control+minus >> log
 bindsym Mod4+control+plus resize  grow  height 5 px or 5 ppt; resize  grow  width  5 px or 5 ppt; $exec echo Mod4+control+plus >> log
 bindsym Mod4+control+Shift+minus resize shrink height 1 px or 1 ppt; resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+minus >> log
 bindsym Mod4+control+Shift+plus resize  grow  height 1 px or 1 ppt; resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+plus >> log

 # save layout
 bindsym Mod4+control+s exec "i3-save-tree > ~/.workspaces/stamp.json; emacsclient -create-frame --alternate-editor='' ~/.workspaces/stamp.json"; $exec echo Mod4+control+s >> log

 # wasd split and move
 bindsym Mod4+control+Shift+w focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+w >> log
 bindsym Mod4+control+Shift+a focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+a >> log
 bindsym Mod4+control+Shift+s focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+s >> log
 bindsym Mod4+control+Shift+d focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+d >> log

 # f-fake fullscren
 bindsym Mod4+control+Shift+f mode $def $exec "sleep 0.5; xdotool key f; sleep 0.25; i3-msg fullscreen disable"; $exec $alert $wrt; $exec echo Mod4+control+Shift+f >> log

 # start dmenu for applications
 bindsym Mod4+Shift+exclam mode $def $exec $dmenu; $exec $alert $wrt; $exec echo Mod4+Shift+exclam >> log

 # start dmenu for commands
 bindsym Mod4+control+e mode $def $exec dmenu_run; $exec $alert $wrt; $exec echo Mod4+control+e >> log

 # application menu
 bindsym Mod4+Menu mode $hov exec morc_menu; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Menu >> log

 # no border touch
 bindsym Mod4+Shift+space mode $tch $no_border; $exec $alert $tch & $touchpad_on; $exec echo Mod4+Shift+space >> log

 # browse workspaces
 bindsym Mod4+Tab mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Tab >> log
 bindsym Mod4+Shift+Tab mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Shift+Tab >> log
 bindsym Mod4+j mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+j >> log
 bindsym Mod4+agrave mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+agrave >> log
 bindsym Mod4+Home mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Home >> log
 bindsym Mod4+End mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+End >> log
 bindsym Mod4+ugrave mode $hov workspace back_and_forth; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+ugrave >> log

 # default focus
 bindsym Mod4+Up mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Up >> log
 bindsym Mod4+Left mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Left >> log
 bindsym Mod4+Down mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Down >> log
 bindsym Mod4+Right mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+Right >> log

 # oklò focus
 bindsym Mod4+o mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+o >> log
 bindsym Mod4+k mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+k >> log
 bindsym Mod4+l mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+l >> log
 bindsym Mod4+ograve mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+ograve >> log

 # switch workspace
 bindsym Mod4+1 mode $hov workspace $w1; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+1 >> log
 bindsym Mod4+2 mode $hov workspace $w2; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+2 >> log
 bindsym Mod4+3 mode $hov workspace $w3; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+3 >> log
 bindsym Mod4+4 mode $hov workspace $w4; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+4 >> log
 bindsym Mod4+5 mode $hov workspace $w5; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+5 >> log
 bindsym Mod4+6 mode $hov workspace $w6; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+6 >> log
 bindsym Mod4+7 mode $hov workspace $w7; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+7 >> log
 bindsym Mod4+8 mode $hov workspace $w8; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+8 >> log
 bindsym Mod4+9 mode $hov workspace $w9; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+9 >> log

 # wasd focus
 bindsym Mod4+w mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+w >> log
 bindsym Mod4+a mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+a >> log
 bindsym Mod4+s mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+s >> log
 bindsym Mod4+d mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+d >> log

 # cut
 bindsym Mod4+control+x mode $hov move container to workspace $wx; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+control+x >> log

 # kill
 bindsym Mod4+control+w mode $hov kill; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+control+w >> log

 # paste
 bindsym Mod4+control+v mode $hov $exec "i3-msg 'workspace --no-auto-back-and-forth $wx; move container to workspace $wx; workspace $wx'"; $refresh_status_bar; $exec $alert $hov & $oklò_enable; $exec echo Mod4+control+v >> log

 # unused keys
 bindsym backslash nop; $exec echo backslash >> log
 bindsym 1 nop; $exec echo 1 >> log
 bindsym 2 nop; $exec echo 2 >> log
 bindsym 3 nop; $exec echo 3 >> log
 bindsym 4 nop; $exec echo 4 >> log
 bindsym 5 nop; $exec echo 5 >> log
 bindsym 6 nop; $exec echo 6 >> log
 bindsym 7 nop; $exec echo 7 >> log
 bindsym 8 nop; $exec echo 8 >> log
 bindsym 9 nop; $exec echo 9 >> log
 bindsym 0 nop; $exec echo 0 >> log
 bindsym apostrophe nop; $exec echo apostrophe >> log
 bindsym igrave nop; $exec echo igrave >> log
 bindsym Tab nop; $exec echo Tab >> log
 bindsym w nop; $exec echo w >> log
 bindsym e nop; $exec echo e >> log
 bindsym t nop; $exec echo t >> log
 bindsym y nop; $exec echo y >> log
 bindsym u nop; $exec echo u >> log
 bindsym i nop; $exec echo i >> log
 bindsym o nop; $exec echo o >> log
 bindsym p nop; $exec echo p >> log
 bindsym egrave nop; $exec echo egrave >> log
 bindsym plus nop; $exec echo plus >> log
 bindsym "Caps_Lock" nop; $exec echo "Caps_Lock" >> log
 bindsym a nop; $exec echo a >> log
 bindsym d nop; $exec echo d >> log
 bindsym f nop; $exec echo f >> log
 bindsym g nop; $exec echo g >> log
 bindsym j nop; $exec echo j >> log
 bindsym k nop; $exec echo k >> log
 bindsym l nop; $exec echo l >> log
 bindsym ograve nop; $exec echo ograve >> log
 bindsym agrave nop; $exec echo agrave >> log
 bindsym ugrave nop; $exec echo ugrave >> log
 bindsym "Shift_L" nop; $exec echo "Shift_L" >> log
 bindsym less nop; $exec echo less >> log
 bindsym z nop; $exec echo z >> log
 bindsym x nop; $exec echo x >> log
 bindsym c nop; $exec echo c >> log
 bindsym v nop; $exec echo v >> log
 bindsym b nop; $exec echo b >> log
 bindsym n nop; $exec echo n >> log
 bindsym m nop; $exec echo m >> log
 bindsym comma nop; $exec echo comma >> log
 bindsym period nop; $exec echo period >> log
 bindsym minus nop; $exec echo minus >> log
 bindsym "Shift_R" nop; $exec echo "Shift_R" >> log
 bindsym "Control_L" nop; $exec echo "Control_L" >> log
 bindsym "Super_L" nop; $exec echo "Super_L" >> log
 bindsym $alt_gr nop; $exec echo $alt_gr >> log
 bindsym "Control_R" nop; $exec echo "Control_R" >> log
 bindsym Up nop; $exec echo Up >> log
 bindsym Left nop; $exec echo Left >> log
 bindsym Down nop; $exec echo Down >> log
 bindsym Right nop; $exec echo Right >> log

 }

 #                                TOUCH-MODE                                   #

 mode $tch {

 bindsym Mod4+c $exec $touchpad_off; mode $cnf $exec $alert $cnf; $exec echo Mod4+c >> log

 bindsym Mod1+Menu $exec $touchpad_off; mode $hov $exec $alert $hov & $oklò_enable; $exec echo Mod1+Menu >> log
 bindsym Menu $exec $touchpad_off; mode $hov $exec $alert $hov & $oklò_enable; $exec echo Menu >> log

 bindsym XF86PowerOff $exec $touchpad_off; mode $pow $exec $alert $pow, fullscreen disable; $exec echo XF86PowerOff >> log

 bindsym Mod4+r $exec $touchpad_off; mode $red $exec $alert $red; $exec echo Mod4+r >> log

 bindsym Mod4+z $exec $touchpad_off; mode $str $exec $alert $str; $exec echo Mod4+z >> log

 bindsym Return $exec $touchpad_off; mode $def $exec $alert $wrt; $exec echo Return >> log
 bindsym BackSpace $exec $touchpad_off; mode $def $exec $alert $wrt; $exec echo BackSpace >> log
 bindsym space $exec $touchpad_off; mode $def $exec $alert $wrt; $exec echo space >> log
 bindsym Delete $exec $touchpad_off; mode $def $exec $alert $wrt; $exec echo Delete >> log

 # borders
 bindsym --border button2 $no_border; $exec echo --border button2 >> log

 # display
 bindsym XF86Display $exec xrandr --output eDP1 --mode 1920x1080 --preferred; $exec echo XF86Display >> log

 # screen brightness controls
 bindsym XF86MonBrightnessUp $brightness_up; $exec echo XF86MonBrightnessUp >> log
 bindsym XF86MonBrightnessDown $brightness_down; $exec echo XF86MonBrightnessDown >> log

 # screen poweroff
 bindsym XF86ScreenSaver $exec xset dpms force off; $exec echo XF86ScreenSaver >> log

 # screenshot
 bindsym --release Print $exec i3-scrot; $exec echo --release Print >> log
 bindsym --release Mod4+Print $exec i3-scrot -w; $exec echo --release Mod4+Print >> log
 bindsym --release Mod4+Shift+Print $exec i3-scrot -s; $exec echo --release Mod4+Shift+Print >> log

 # touchpad toggle
 bindsym XF86TouchpadToggle $exec $touchpad_toggle; $exec echo XF86TouchpadToggle >> log

 # no border write
 bindsym Shift+"Super_L" $exec $touchpad_off; mode $def $no_border; $exec $alert $wrt; $exec echo Shift+"Super_L" >> log

 # no border hover
 bindsym Shift+Menu $exec $touchpad_off; mode $hov $no_border; $exec $alert $hov & $oklò_enable; $exec echo Shift+Menu >> log

 # where am i?
 bindsym --release "Super_L" $exec $touchpad_off; mode $hov $border, $focus_one, fullscreen disable; $exec $touchpad_off; $exec $alert $hov & $oklò_enable; $exec echo --release "Super_L" >> log

 # kill
 bindsym Mod1+F4 kill; $refresh_status_bar; $exec echo Mod1+F4 >> log

 # borders
 bindsym Mod4+b $border; $exec echo Mod4+b >> log
 bindsym Mod4+Shift+b $no_border; $exec echo Mod4+Shift+b >> log

 # default move
 bindsym Mod4+Shift+Up move up; $exec echo Mod4+Shift+Up >> log
 bindsym Mod4+Shift+Left move left; $exec echo Mod4+Shift+Left >> log
 bindsym Mod4+Shift+Down move down; $exec echo Mod4+Shift+Down >> log
 bindsym Mod4+Shift+Right move right; $exec echo Mod4+Shift+Right >> log

 # focus group
 bindsym Mod4+less focus child; $exec echo Mod4+less >> log
 bindsym Mod4+Shift+greater focus parent; $exec echo Mod4+Shift+greater >> log

 # fullscreen
 bindsym Mod4+f fullscreen toggle; $exec echo Mod4+f >> log
 bindsym Mod4+Shift+f $exec "sleep 0.5; xdotool key F11; sleep 0.25; i3-msg fullscreen disable"; $exec echo Mod4+Shift+f >> log

 # oklò move
 bindsym Mod4+Shift+o move up; $exec echo Mod4+Shift+o >> log
 bindsym Mod4+Shift+k move left; $exec echo Mod4+Shift+k >> log
 bindsym Mod4+Shift+l move down; $exec echo Mod4+Shift+l >> log
 bindsym Mod4+Shift+ograve move right; $exec echo Mod4+Shift+ograve >> log

 # open emacs
 bindsym Mod4+e exec $emacs; $exec echo Mod4+e >> log

 # open terminal
 bindsym Mod4+Return exec gnome-terminal; $exec echo Mod4+Return >> log

 # resize window
 bindsym Mod4+i resize shrink height 5 px or 5 ppt; $exec echo Mod4+i >> log
 bindsym Mod4+p resize  grow  height 5 px or 5 ppt; $exec echo Mod4+p >> log
 bindsym Mod4+Prior resize shrink height 5 px or 5 ppt; $exec echo Mod4+Prior >> log
 bindsym Mod4+Next resize  grow  height 5 px or 5 ppt; $exec echo Mod4+Next >> log
 bindsym Mod4+Shift+i resize shrink width  5 px or 5 ppt; $exec echo Mod4+Shift+i >> log
 bindsym Mod4+Shift+p resize  grow  width  5 px or 5 ppt; $exec echo Mod4+Shift+p >> log
 bindsym Mod4+Shift+Prior resize shrink width  5 px or 5 ppt; $exec echo Mod4+Shift+Prior >> log
 bindsym Mod4+Shift+Next resize  grow  width  5 px or 5 ppt; $exec echo Mod4+Shift+Next >> log

 # split orientation
 bindsym Mod4+v split v; exec notify-send 'vertical'; $exec echo Mod4+v >> log
 bindsym Mod4+h split h; exec notify-send 'horizontal'; $exec echo Mod4+h >> log
 bindsym Mod4+g split v; focus parent; layout toggle split; focus child; $exec echo Mod4+g >> log

 # wasd move
 bindsym Mod4+Shift+w move up; $exec echo Mod4+Shift+w >> log
 bindsym Mod4+Shift+a move left; $exec echo Mod4+Shift+a >> log
 bindsym Mod4+Shift+s move down; $exec echo Mod4+Shift+s >> log
 bindsym Mod4+Shift+d move right; $exec echo Mod4+Shift+d >> log

 # default split and move
 bindsym Mod4+control+Shift+Up focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+Up >> log
 bindsym Mod4+control+Shift+Left focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+Left >> log
 bindsym Mod4+control+Shift+Down focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+Down >> log
 bindsym Mod4+control+Shift+Right focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+Right >> log

 # files
 bindsym Mod4+control+f $exec $fm; $exec echo Mod4+control+f >> log
 bindsym Mod4+control+j $exec $fm Downloads; $exec echo Mod4+control+j >> log

 # focus all
 bindsym Mod4+control+a $focus_all; $exec echo Mod4+control+a >> log

 # move container while browsing workspaces
 bindsym Mod4+control+Tab move container to workspace next; workspace next; $exec echo Mod4+control+Tab >> log
 bindsym Mod4+control+Shift+Tab move container to workspace prev; workspace prev; $exec echo Mod4+control+Shift+Tab >> log
 bindsym Mod4+control+Home move container to workspace prev; workspace prev; $exec echo Mod4+control+Home >> log
 bindsym Mod4+control+End move container to workspace next; workspace next; $exec echo Mod4+control+End >> log
 bindsym Mod4+control+ugrave move container to workspace back_and_forth; workspace back_and_forth; $exec echo Mod4+control+ugrave >> log

 # move focused container to workspace and follow
 bindsym Mod4+control+1 move container to workspace $w1; workspace $w1; $exec echo Mod4+control+1 >> log
 bindsym Mod4+control+2 move container to workspace $w2; workspace $w2; $exec echo Mod4+control+2 >> log
 bindsym Mod4+control+3 move container to workspace $w3; workspace $w3; $exec echo Mod4+control+3 >> log
 bindsym Mod4+control+4 move container to workspace $w4; workspace $w4; $exec echo Mod4+control+4 >> log
 bindsym Mod4+control+5 move container to workspace $w5; workspace $w5; $exec echo Mod4+control+5 >> log
 bindsym Mod4+control+6 move container to workspace $w6; workspace $w6; $exec echo Mod4+control+6 >> log
 bindsym Mod4+control+7 move container to workspace $w7; workspace $w7; $exec echo Mod4+control+7 >> log
 bindsym Mod4+control+8 move container to workspace $w8; workspace $w8; $exec echo Mod4+control+8 >> log
 bindsym Mod4+control+9 move container to workspace $w9; workspace $w9; $exec echo Mod4+control+9 >> log

 # oklò split and move
 bindsym Mod4+control+Shift+o focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+o >> log
 bindsym Mod4+control+Shift+k focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+k >> log
 bindsym Mod4+control+Shift+l focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+l >> log
 bindsym Mod4+control+Shift+ograve focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+ograve >> log

 # reload configuration
 bindsym Mod4+control+r $exec "make -C ~/.i3 && i3-msg $no_border restart"; $exec echo Mod4+control+r >> log
 bindsym Mod4+control+Shift+r $exec "make -C ~/.i3 stable && i3-msg $no_border restart"; $exec echo Mod4+control+Shift+r >> log

 # resize (precise)
 bindsym Mod4+control+i resize shrink height 1 px or 1 ppt; $exec echo Mod4+control+i >> log
 bindsym Mod4+control+p resize  grow  height 1 px or 1 ppt; $exec echo Mod4+control+p >> log
 bindsym Mod4+control+Prior resize shrink height 1 px or 1 ppt; $exec echo Mod4+control+Prior >> log
 bindsym Mod4+control+Next resize  grow  height 1 px or 1 ppt; $exec echo Mod4+control+Next >> log
 bindsym Mod4+control+Shift+i resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+i >> log
 bindsym Mod4+control+Shift+p resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+p >> log
 bindsym Mod4+control+Shift+Prior resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+Prior >> log
 bindsym Mod4+control+Shift+Next resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+Next >> log

 # resize (scale)
 bindsym Mod4+control+minus resize shrink height 5 px or 5 ppt; resize shrink width  5 px or 5 ppt; $exec echo Mod4+control+minus >> log
 bindsym Mod4+control+plus resize  grow  height 5 px or 5 ppt; resize  grow  width  5 px or 5 ppt; $exec echo Mod4+control+plus >> log
 bindsym Mod4+control+Shift+minus resize shrink height 1 px or 1 ppt; resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+minus >> log
 bindsym Mod4+control+Shift+plus resize  grow  height 1 px or 1 ppt; resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+plus >> log

 # save layout
 bindsym Mod4+control+s exec "i3-save-tree > ~/.workspaces/stamp.json; emacsclient -create-frame --alternate-editor='' ~/.workspaces/stamp.json"; $exec echo Mod4+control+s >> log

 # wasd split and move
 bindsym Mod4+control+Shift+w focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+w >> log
 bindsym Mod4+control+Shift+a focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+a >> log
 bindsym Mod4+control+Shift+s focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+s >> log
 bindsym Mod4+control+Shift+d focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+d >> log

 # f-fake fullscren
 bindsym Mod4+control+Shift+f $exec $touchpad_off; mode $def $exec "sleep 0.5; xdotool key f; sleep 0.25; i3-msg fullscreen disable"; $exec $alert $wrt; $exec echo Mod4+control+Shift+f >> log

 # start dmenu for applications
 bindsym Mod4+Shift+exclam $exec $touchpad_off; mode $def $exec $dmenu; $exec $alert $wrt; $exec echo Mod4+Shift+exclam >> log

 # start dmenu for commands
