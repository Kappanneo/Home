 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                               SETTINGS                                      #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 
 # Set mod key (Mod1=<Alt>, Mod4=<Super>)
 set $mod Mod4
 
 # set default desktop layout (default is tiling)
 # workspace_layout tabbed <stacking|tabbed>
 
 # Configure border style <normal|1pixel|pixel xx|none|pixel>
 new_window pixel 0
 new_float normal
 
 # Hide borders
 hide_edge_borders none
 
 # Font for window titles. Will also be used by the bar unless a different font
 # is used in the bar {} block below.
 font pango:DejaVu Sans Mono 8

 # go back to previous workspace if you try to go to the current one
 # (allow for fast window movement to a workspace and back)
 workspace_auto_back_and_forth yes

 # focus on mouseover (is a pain if you happen to have a mouse)
 focus_follows_mouse no

 # I like the original snake better
 focus_wrapping no

 # smart|ignore|leave_fullscreen
 popup_during_fullscreen leave_fullscreen

 # smart|urgent|focus|none
 focus_on_window_activation none

 #                                 COLORS                                      #

 # class                 border  backgr. text    ind.    child
 client.focused          #4CC97E #4CC97E #ffffff #4CC97E #4CC97E
 client.focused_inactive #222222 #222222 #ffffff #222222 #222222
 client.unfocused        #222222 #222222 #888888 #222222 #222222
 client.urgent           #222222 #900000 #ffffff #900000 #900000
 client.placeholder      #000000 #222222 #ffffff #000000 #222222

 client.background       #222222

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                               STATUS-BAR                                    #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 bar {
        status_command    ./.bin/my_status.sh
        position          bottom
        mode              dock
        workspace_buttons yes
        colors {
                   background #000000
                   statusline #999999
                   focused_workspace  #ffffff #000000
                   active_workspace   #ffffff #000000
                   inactive_workspace #888888 #000000
                   urgent_workspace   #ffffff #000000
               }

     # disable scrolling on status bar
        bindsym button4 nop # scroll wheel up
        bindsym button5 nop # scroll wheel down

     # no border on middle mouse
        bindsym button2 $no_border
 }

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                               DEFINITIONS                                   #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 # The (new) i3-dmenu-desktop which only displays applications
 # shipping a .desktop file. It is a wrapper around dmenu, so you need that
 # installed.
 set $dmenu i3-dmenu-desktop

 # file manager
 set $fm nautilus -w

 # for brevity
 set $exec exec --no-startup-id
 set $exec_always exec_always --no-startup-id
 set $alert notify-send --expire-time=1200
 set $refresh_status_bar exec killall -USR1 i3status
 set $focus_all focus parent; focus parent; focus parent; focus parent
 set $focus_one focus child ; focus child ; focus child ; focus child
 set $no_border [tiling] border none
 set $border [tiling] border pixel 5
 set $alt_gr "ISO_Level3_Shift"
 set $emacs emacsclient -create-frame --alternate-editor=""

 #                                KEYBINDINGS                                  #

 set $disable_Caps setxkbmap -option ctrl:nocaps
 set $enable_Caps setxkbmap -option
 set $oklò_disable ~/.bin/oklò.sh 0
 set $oklò_enable ~/.bin/oklò.sh 1

 #                              FUNCTIONALITIES                                #

 # touchpad toggle
 set $touchpad_on ~/.bin/touchpad_toggle.sh 1 & xdotool mousemove 800 450
 set $touchpad_off ~/.bin/touchpad_toggle.sh 0 & xdotool mousemove 2500 1200
 set $touchpad_off_2 ~/.bin/touchpad_toggle.sh 0
 set $touchpad_toggle ~/.bin/touchpad_toggle.sh

 # autoscroll
 set $disable_autoscroll ~/.bin/autoscroll.sh 0
 set $enable_autoscroll ~/.bin/autoscroll.sh 1

 # brightness
 set $brightness_up exec "xbacklight -inc 5; notify-send 'brightness up'"
 set $brightness_down exec "xbacklight -dec 5; notify-send 'brightness down'"

 #                                 WORKSPACES                                  #

 set $w1 1nternet
 set $w2 2rectory
 set $w3 3code
 set $w4 4terminl
 set $w5 5team
 set $w6 6iscord
 set $w7 7atex
 set $w8 8volante
 set $w9 9imp


 # this one is for cut and paste
 set $wx X


 #                                  LAYOUTS                                    #

 set $layout_1 i3-msg 'workspace --no-auto-back-and-forth 1nternet; split h; focus parent; focus parent; focus parent; focus parent; kill; append_layout ~/.workspaces/1nternet.json'
 set $layout_2 i3-msg 'workspace --no-auto-back-and-forth 2rectory; split h; focus parent; focus parent; focus parent; focus parent; focus parent; kill; append_layout ~/.workspaces/2rectory.json'

 set $fill_1 chromium --new-window --profile-directory=Default https://web.telegram.org/ https://web.whatsapp.com/ & sleep 0.25; chromium --new-window --profile-directory=Default https://mail.google.com/mail/u/0/ https://mail.google.com/mail/u/1/ https://calendar.google.com/calendar/ & sleep 0.5; chromium --new-window --profile-directory=Default https://keep.google.com/u/0/ https://getpocket.com/a/queue/list/ & sleep 1.5; i3-msg [class='.?'] border none
 set $fill_2 nautilus -w & nautilus -w Downloads & emacsclient -create-frame --alternate-editor='' ~/memo & chromium --new-window --profile-directory=Default https://github.com/Kappanneo https://gitlab.com/kappanneo & gnome-terminal & sleep 1; i3-msg [class='.?'] border none

 #                                    MODES                                    #

 set $def "default"
 set $cnf "CONFG: [c]onfigure i3  .git[i]gnore  [a]pplications  [e]macs  [s]tatusbar  [z]sh  [g]uide  [1|2|l]ayouts  [r]edshift  [space|esc] exit mode"
 set $hov "HOVER: writing disabled  [oklò] move cursor  [0] insert  [space|esc] write mode"
 set $pow "POWER: [q]uit  [r]estart  [s]uspend  [h]ibernate  [esc]"
 set $red "REDSH: [123] shift red level  [+] increase  [space|esc] exit mode"
 set $str "START: [1|2] layouts  [spaec|esc] exit mode"
 set $tch "TOUCH: touchpad enabled  [space|esc] write mode"
 set $wrt "WRITE: writing enabled"


 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                APPLICATIONS                                 #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 # code
 assign [class=".*code.*"] $w3

 # android studio
 assign [class="jetbrains-studio"] $w3

 # steam
 assign [class="Steam"] $w5
 assign [class="Wine"] $w5
 for_window [title="Steam - News"] kill
 for_window [title="Friends"] resize shrink width 22 px or 22 ppt; split h
 for_window [title="Steam"] split h

 # discord and skype
 assign [class="discord"] $w6
 assign [title="Telegram"] $w6
 assign [title="Skype"] $w6

 # texmaker
 assign [title="Texmaker"] $w7
 assign [title=".*overleaf.*"] $w7

 # gimp
 assign [class="Gimp.*"] $w9

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                ON-NEW-WINDOW                                #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 for_window [class=".?" title=".?"] split toggle, fullscreen disable, $border

 for_window [floating] floating disable

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                  STARTUP                                    #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 # keybinding
 $exec $disable_Caps

 # start with touchpad disabled
 $exec $touchpad_off

 # background (not always permanent, may need reaload)
 $exec_always $enable_autoscroll
 $exec_always nitrogen --restore
 $exec_always fix_xcursor

 # background applications
 $exec /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1
 $exec nitrogen --restore; sleep 1; compton -b
 $exec xfce4-power-manager
 # $exec start_conky_maja
 $exec alttab -s 1 -d 1
 
 # bar applications
 $exec nm-applet
 $exec pamac-tray
 $exec pa-applet
 $exec clipit

 # reshift oneshot mode (to set starting temperature according to time)
 $exec redshift -o

 # premade layout
 $exec $layout_2; $fill_2

 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                  COMMANDS                                   #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 # borders
 bindsym --border button2 $no_border

 # display
 bindsym XF86Display $exec xrandr --output eDP1 --mode 1920x1080 --preferred

 # screen brightness controls
 bindsym XF86MonBrightnessUp $brightness_up
 bindsym XF86MonBrightnessDown $brightness_down

 # screen poweroff
 bindsym XF86ScreenSaver $exec xset dpms force off

 # screenshot
 bindsym --release Print $exec i3-scrot

 # touchpad toggle
 bindsym XF86TouchpadToggle $exec $touchpad_toggle

 # no border write
 bindsym Shift+"Super_L" $no_border

 # no border hover
 bindsym Shift+Menu mode $hov $no_border; $exec $alert $hov & $oklò_enable

 # where am i?
 bindsym --release "Super_L" mode $hov $border, $focus_one, fullscreen disable; $exec $touchpad_off; $exec $alert $hov & $oklò_enable



 #                                ALT-COMMANDS                                 #

 # kill
 bindsym Mod1+F4 kill; $refresh_status_bar



 #                               SUPER-COMMANDS                                #

 # borders
 bindsym Mod4+b $border
 bindsym Mod4+Shift+b $no_border

 # browse workspaces
 bindsym Mod4+j workspace prev
 bindsym Mod4+agrave workspace next
 bindsym Mod4+ugrave workspace back_and_forth

 # default move
 bindsym Mod4+Shift+Up move up
 bindsym Mod4+Shift+Left move left
 bindsym Mod4+Shift+Down move down
 bindsym Mod4+Shift+Right move right

 # focus group
 bindsym Mod4+less focus child
 bindsym Mod4+Shift+greater focus parent

 # fullscreen
 bindsym Mod4+f fullscreen toggle
 bindsym Mod4+Shift+f $exec "sleep 0.5; xdotool key F11; sleep 0.5; i3-msg fullscreen disable"

 # oklò move
 bindsym Mod4+Shift+o move up
 bindsym Mod4+Shift+k move left
 bindsym Mod4+Shift+l move down
 bindsym Mod4+Shift+ograve move right

 # open emacs
 bindsym Mod4+e exec $emacs

 # open terminal
 bindsym Mod4+Return exec gnome-terminal

 # resize window
 bindsym Mod4+i resize shrink height 5 px or 5 ppt
 bindsym Mod4+p resize  grow  height 5 px or 5 ppt
 bindsym Mod4+Prior resize shrink height 5 px or 5 ppt
 bindsym Mod4+Next resize  grow  height 5 px or 5 ppt
 bindsym Mod4+Shift+i resize shrink width  5 px or 5 ppt
 bindsym Mod4+Shift+p resize  grow  width  5 px or 5 ppt
 bindsym Mod4+Shift+Prior resize shrink width  5 px or 5 ppt
 bindsym Mod4+Shift+Next resize  grow  width  5 px or 5 ppt

 # split orientation
 bindsym Mod4+v split v; exec notify-send 'vertical'
 bindsym Mod4+h split h; exec notify-send 'horizontal'
 bindsym Mod4+g split v; focus parent; layout toggle split; focus child

 # wasd move
 bindsym Mod4+Shift+w move up
 bindsym Mod4+Shift+a move left
 bindsym Mod4+Shift+s move down
 bindsym Mod4+Shift+d move right

 # files
 bindsym Mod4+control+f $exec $fm
 bindsym Mod4+control+j $exec $fm Downloads

 # focus all
 bindsym Mod4+control+a $focus_all

 # move focused container to workspace and follow
 bindsym Mod4+control+1 move container to workspace $w1; workspace $w1
 bindsym Mod4+control+2 move container to workspace $w2; workspace $w2
 bindsym Mod4+control+3 move container to workspace $w3; workspace $w3
 bindsym Mod4+control+4 move container to workspace $w4; workspace $w4
 bindsym Mod4+control+5 move container to workspace $w5; workspace $w5
 bindsym Mod4+control+6 move container to workspace $w6; workspace $w6
 bindsym Mod4+control+7 move container to workspace $w7; workspace $w7
 bindsym Mod4+control+8 move container to workspace $w8; workspace $w8
 bindsym Mod4+control+9 move container to workspace $w9; workspace $w9

 # reload configuration
 bindsym Mod4+control+r $exec "make -C ~/.i3 && i3-msg $no_border restart"
 bindsym Mod4+control+Shift+r $exec "make -C ~/.i3 stable && i3-msg $no_border restart"

 # resize (precise)
 bindsym Mod4+control+i resize shrink height 1 px or 1 ppt
 bindsym Mod4+control+p resize  grow  height 1 px or 1 ppt
 bindsym Mod4+control+Prior resize shrink height 1 px or 1 ppt
 bindsym Mod4+control+Next resize  grow  height 1 px or 1 ppt
 bindsym Mod4+control+Shift+i resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+p resize  grow  width  1 px or 1 ppt
 bindsym Mod4+control+Shift+Prior resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+Next resize  grow  width  1 px or 1 ppt

 # resize (scale)
 bindsym Mod4+control+minus resize shrink height 5 px or 5 ppt; resize shrink width  5 px or 5 ppt
 bindsym Mod4+control+plus resize  grow  height 5 px or 5 ppt; resize  grow  width  5 px or 5 ppt
 bindsym Mod4+control+Shift+minus resize shrink height 1 px or 1 ppt; resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+plus resize  grow  height 1 px or 1 ppt; resize  grow  width  1 px or 1 ppt

 # save layout
 bindsym Mod4+control+s exec "i3-save-tree > ~/.workspaces/stamp.json; $emacs ~/.workspaces/stamp.json"

 # start dmenu for applications
 bindsym Mod4+Shift+exclam $exec $dmenu

 # start dmenu for commands
 bindsym Mod4+control+e $exec dmenu_run

 # application menu
 bindsym Mod4+Menu mode $hov exec morc_menu; $exec $alert $hov & $oklò_enable

 # no border touch
 bindsym Mod4+Shift+space mode $tch $no_border; $exec $alert $tch & $touchpad_on

 # browse workspaces
 bindsym Mod4+Tab mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Shift+Tab mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # default focus
 bindsym Mod4+Up mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Left mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Down mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Right mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # oklò focus
 bindsym Mod4+o mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+k mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+l mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+ograve mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # switch workspace
 bindsym Mod4+1 mode $hov workspace $w1; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+2 mode $hov workspace $w2; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+3 mode $hov workspace $w3; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+4 mode $hov workspace $w4; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+5 mode $hov workspace $w5; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+6 mode $hov workspace $w6; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+7 mode $hov workspace $w7; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+8 mode $hov workspace $w8; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+9 mode $hov workspace $w9; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # wasd focus
 bindsym Mod4+w mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+a mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+s mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+d mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # cut
 bindsym Mod4+control+x mode $hov move container to workspace $wx; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # kill
 bindsym Mod4+control+w mode $hov kill; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # paste
 bindsym Mod4+control+v mode $hov $exec "i3-msg 'workspace --no-auto-back-and-forth $wx; move container to workspace $wx; workspace $wx'"; $refresh_status_bar; $exec $alert $hov & $oklò_enable



 #                                   MODES                                     #

 bindsym Mod4+c mode $cnf $exec $alert $cnf

 bindsym Mod1+Menu mode $hov $exec $alert $hov & $oklò_enable
 bindsym Menu mode $hov $exec $alert $hov & $oklò_enable

 bindsym XF86PowerOff mode $pow $exec $alert $pow, fullscreen disable

 bindsym Mod4+r mode $red $exec $alert $red

 bindsym Mod4+z mode $str $exec $alert $str

 bindsym Mod4+space mode $tch $exec $alert $tch & $touchpad_on



 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
 #                                   MODES                                     #
 # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

 #                               POWEROFF-MODE                                 #

 mode $pow {

 # options
 bindsym XF86PowerOff mode $def $exec $alert $pow; $exec $alert $wrt
 bindsym q mode $def $exec systemctl poweroff; $exec $alert $wrt
 bindsym r mode $def $exec systemctl reboot; $exec $alert $wrt
 bindsym s mode $def $exec systemctl suspend; $exec $alert $wrt
 bindsym h mode $def $exec systemctl hibernate; $exec $alert $wrt

 bindsym Mod4+c mode $cnf $exec $alert $cnf

 bindsym Mod1+Menu mode $hov $exec $alert $hov & $oklò_enable
 bindsym Menu mode $hov $exec $alert $hov & $oklò_enable

 bindsym Mod4+r mode $red $exec $alert $red

 bindsym Mod4+z mode $str $exec $alert $str

 bindsym Mod4+space mode $tch $exec $alert $tch & $touchpad_on

 bindsym Return mode $def $exec $alert $wrt
 bindsym BackSpace mode $def $exec $alert $wrt
 bindsym space mode $def $exec $alert $wrt
 bindsym Delete mode $def $exec $alert $wrt
 bindsym "Alt_L" mode $def $exec $alert $wrt

 # borders
 bindsym --border button2 $no_border

 # display
 bindsym XF86Display $exec xrandr --output eDP1 --mode 1920x1080 --preferred

 # screen brightness controls
 bindsym XF86MonBrightnessUp $brightness_up
 bindsym XF86MonBrightnessDown $brightness_down

 # screen poweroff
 bindsym XF86ScreenSaver $exec xset dpms force off

 # screenshot
 bindsym --release Print $exec i3-scrot

 # touchpad toggle
 bindsym XF86TouchpadToggle $exec $touchpad_toggle

 # no border write
 bindsym Shift+"Super_L" mode $def $no_border; $exec $alert $wrt

 # no border hover
 bindsym Shift+Menu mode $hov $no_border; $exec $alert $hov & $oklò_enable

 # where am i?
 bindsym --release "Super_L" mode $hov $border, $focus_one, fullscreen disable; $exec $touchpad_off; $exec $alert $hov & $oklò_enable

 # kill
 bindsym Mod1+F4 kill; $refresh_status_bar

 # borders
 bindsym Mod4+b $border
 bindsym Mod4+Shift+b $no_border

 # browse workspaces
 bindsym Mod4+j workspace prev
 bindsym Mod4+agrave workspace next
 bindsym Mod4+ugrave workspace back_and_forth

 # default move
 bindsym Mod4+Shift+Up move up
 bindsym Mod4+Shift+Left move left
 bindsym Mod4+Shift+Down move down
 bindsym Mod4+Shift+Right move right

 # focus group
 bindsym Mod4+less focus child
 bindsym Mod4+Shift+greater focus parent

 # fullscreen
 bindsym Mod4+f fullscreen toggle
 bindsym Mod4+Shift+f $exec "sleep 0.5; xdotool key F11; sleep 0.5; i3-msg fullscreen disable"

 # oklò move
 bindsym Mod4+Shift+o move up
 bindsym Mod4+Shift+k move left
 bindsym Mod4+Shift+l move down
 bindsym Mod4+Shift+ograve move right

 # open emacs
 bindsym Mod4+e exec $emacs

 # open terminal
 bindsym Mod4+Return exec gnome-terminal

 # resize window
 bindsym Mod4+i resize shrink height 5 px or 5 ppt
 bindsym Mod4+p resize  grow  height 5 px or 5 ppt
 bindsym Mod4+Prior resize shrink height 5 px or 5 ppt
 bindsym Mod4+Next resize  grow  height 5 px or 5 ppt
 bindsym Mod4+Shift+i resize shrink width  5 px or 5 ppt
 bindsym Mod4+Shift+p resize  grow  width  5 px or 5 ppt
 bindsym Mod4+Shift+Prior resize shrink width  5 px or 5 ppt
 bindsym Mod4+Shift+Next resize  grow  width  5 px or 5 ppt

 # split orientation
 bindsym Mod4+v split v; exec notify-send 'vertical'
 bindsym Mod4+h split h; exec notify-send 'horizontal'
 bindsym Mod4+g split v; focus parent; layout toggle split; focus child

 # wasd move
 bindsym Mod4+Shift+w move up
 bindsym Mod4+Shift+a move left
 bindsym Mod4+Shift+s move down
 bindsym Mod4+Shift+d move right

 # files
 bindsym Mod4+control+f $exec $fm
 bindsym Mod4+control+j $exec $fm Downloads

 # focus all
 bindsym Mod4+control+a $focus_all

 # move focused container to workspace and follow
 bindsym Mod4+control+1 move container to workspace $w1; workspace $w1
 bindsym Mod4+control+2 move container to workspace $w2; workspace $w2
 bindsym Mod4+control+3 move container to workspace $w3; workspace $w3
 bindsym Mod4+control+4 move container to workspace $w4; workspace $w4
 bindsym Mod4+control+5 move container to workspace $w5; workspace $w5
 bindsym Mod4+control+6 move container to workspace $w6; workspace $w6
 bindsym Mod4+control+7 move container to workspace $w7; workspace $w7
 bindsym Mod4+control+8 move container to workspace $w8; workspace $w8
 bindsym Mod4+control+9 move container to workspace $w9; workspace $w9

 # reload configuration
 bindsym Mod4+control+r $exec "make -C ~/.i3 && i3-msg $no_border restart"
 bindsym Mod4+control+Shift+r $exec "make -C ~/.i3 stable && i3-msg $no_border restart"

 # resize (precise)
 bindsym Mod4+control+i resize shrink height 1 px or 1 ppt
 bindsym Mod4+control+p resize  grow  height 1 px or 1 ppt
 bindsym Mod4+control+Prior resize shrink height 1 px or 1 ppt
 bindsym Mod4+control+Next resize  grow  height 1 px or 1 ppt
 bindsym Mod4+control+Shift+i resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+p resize  grow  width  1 px or 1 ppt
 bindsym Mod4+control+Shift+Prior resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+Next resize  grow  width  1 px or 1 ppt

 # resize (scale)
 bindsym Mod4+control+minus resize shrink height 5 px or 5 ppt; resize shrink width  5 px or 5 ppt
 bindsym Mod4+control+plus resize  grow  height 5 px or 5 ppt; resize  grow  width  5 px or 5 ppt
 bindsym Mod4+control+Shift+minus resize shrink height 1 px or 1 ppt; resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+plus resize  grow  height 1 px or 1 ppt; resize  grow  width  1 px or 1 ppt

 # save layout
 bindsym Mod4+control+s exec "i3-save-tree > ~/.workspaces/stamp.json; $emacs ~/.workspaces/stamp.json"

 # start dmenu for applications
 bindsym Mod4+Shift+exclam mode $def $exec $dmenu; $exec $alert $wrt

 # start dmenu for commands
 bindsym Mod4+control+e mode $def $exec dmenu_run; $exec $alert $wrt

 # application menu
 bindsym Mod4+Menu mode $hov exec morc_menu; $exec $alert $hov & $oklò_enable

 # no border touch
 bindsym Mod4+Shift+space mode $tch $no_border; $exec $alert $tch & $touchpad_on

 # browse workspaces
 bindsym Mod4+Tab mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Shift+Tab mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # default focus
 bindsym Mod4+Up mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Left mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Down mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Right mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # oklò focus
 bindsym Mod4+o mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+k mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+l mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+ograve mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # switch workspace
 bindsym Mod4+1 mode $hov workspace $w1; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+2 mode $hov workspace $w2; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+3 mode $hov workspace $w3; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+4 mode $hov workspace $w4; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+5 mode $hov workspace $w5; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+6 mode $hov workspace $w6; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+7 mode $hov workspace $w7; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+8 mode $hov workspace $w8; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+9 mode $hov workspace $w9; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # wasd focus
 bindsym Mod4+w mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+a mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+s mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+d mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # cut
 bindsym Mod4+control+x mode $hov move container to workspace $wx; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # kill
 bindsym Mod4+control+w mode $hov kill; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # paste
 bindsym Mod4+control+v mode $hov $exec "i3-msg 'workspace --no-auto-back-and-forth $wx; move container to workspace $wx; workspace $wx'"; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # unused keys
 bindsym backslash nop
 bindsym 1 nop
 bindsym 2 nop
 bindsym 3 nop
 bindsym 4 nop
 bindsym 5 nop
 bindsym 6 nop
 bindsym 7 nop
 bindsym 8 nop
 bindsym 9 nop
 bindsym 0 nop
 bindsym apostrophe nop
 bindsym igrave nop
 bindsym Tab nop
 bindsym w nop
 bindsym e nop
 bindsym t nop
 bindsym y nop
 bindsym u nop
 bindsym i nop
 bindsym o nop
 bindsym p nop
 bindsym egrave nop
 bindsym plus nop
 bindsym "Caps_Lock" nop
 bindsym a nop
 bindsym d nop
 bindsym f nop
 bindsym g nop
 bindsym j nop
 bindsym k nop
 bindsym l nop
 bindsym ograve nop
 bindsym agrave nop
 bindsym ugrave nop
 bindsym "Shift_L" nop
 bindsym less nop
 bindsym z nop
 bindsym x nop
 bindsym c nop
 bindsym v nop
 bindsym b nop
 bindsym n nop
 bindsym m nop
 bindsym comma nop
 bindsym period nop
 bindsym minus nop
 bindsym "Shift_R" nop
 bindsym "Control_L" nop
 bindsym "Super_L" nop
 bindsym $alt_gr nop
 bindsym "Control_R" nop
 bindsym Up nop
 bindsym Left nop
 bindsym Down nop
 bindsym Right nop

 }

 #                                TOUCH-MODE                                   #

 mode $tch {

 bindsym Mod4+c $exec $touchpad_off; mode $cnf $exec $alert $cnf

 bindsym Mod1+Menu $exec $touchpad_off; mode $hov $exec $alert $hov & $oklò_enable
 bindsym Menu $exec $touchpad_off; mode $hov $exec $alert $hov & $oklò_enable

 bindsym XF86PowerOff $exec $touchpad_off; mode $pow $exec $alert $pow, fullscreen disable

 bindsym Mod4+r $exec $touchpad_off; mode $red $exec $alert $red

 bindsym Mod4+z $exec $touchpad_off; mode $str $exec $alert $str

 bindsym Return $exec $touchpad_off; mode $def $exec $alert $wrt
 bindsym BackSpace $exec $touchpad_off; mode $def $exec $alert $wrt
 bindsym space $exec $touchpad_off; mode $def $exec $alert $wrt
 bindsym Delete $exec $touchpad_off; mode $def $exec $alert $wrt

 # borders
 bindsym --border button2 $no_border

 # display
 bindsym XF86Display $exec xrandr --output eDP1 --mode 1920x1080 --preferred

 # screen brightness controls
 bindsym XF86MonBrightnessUp $brightness_up
 bindsym XF86MonBrightnessDown $brightness_down

 # screen poweroff
 bindsym XF86ScreenSaver $exec xset dpms force off

 # screenshot
 bindsym --release Print $exec i3-scrot

 # touchpad toggle
 bindsym XF86TouchpadToggle $exec $touchpad_toggle

 # no border write
 bindsym Shift+"Super_L" $exec $touchpad_off; mode $def $no_border; $exec $alert $wrt

 # no border hover
 bindsym Shift+Menu $exec $touchpad_off; mode $hov $no_border; $exec $alert $hov & $oklò_enable

 # where am i?
 bindsym --release "Super_L" $exec $touchpad_off; mode $hov $border, $focus_one, fullscreen disable; $exec $touchpad_off; $exec $alert $hov & $oklò_enable

 # kill
 bindsym Mod1+F4 kill; $refresh_status_bar

 # borders
 bindsym Mod4+b $border
 bindsym Mod4+Shift+b $no_border

 # browse workspaces
 bindsym Mod4+j workspace prev
 bindsym Mod4+agrave workspace next
 bindsym Mod4+ugrave workspace back_and_forth

 # default move
 bindsym Mod4+Shift+Up move up
 bindsym Mod4+Shift+Left move left
 bindsym Mod4+Shift+Down move down
 bindsym Mod4+Shift+Right move right

 # focus group
 bindsym Mod4+less focus child
 bindsym Mod4+Shift+greater focus parent

 # fullscreen
 bindsym Mod4+f fullscreen toggle
 bindsym Mod4+Shift+f $exec "sleep 0.5; xdotool key F11; sleep 0.5; i3-msg fullscreen disable"

 # oklò move
 bindsym Mod4+Shift+o move up
 bindsym Mod4+Shift+k move left
 bindsym Mod4+Shift+l move down
 bindsym Mod4+Shift+ograve move right

 # open emacs
 bindsym Mod4+e exec $emacs

 # open terminal
 bindsym Mod4+Return exec gnome-terminal

 # resize window
 bindsym Mod4+i resize shrink height 5 px or 5 ppt
 bindsym Mod4+p resize  grow  height 5 px or 5 ppt
 bindsym Mod4+Prior resize shrink height 5 px or 5 ppt
 bindsym Mod4+Next resize  grow  height 5 px or 5 ppt
 bindsym Mod4+Shift+i resize shrink width  5 px or 5 ppt
 bindsym Mod4+Shift+p resize  grow  width  5 px or 5 ppt
 bindsym Mod4+Shift+Prior resize shrink width  5 px or 5 ppt
 bindsym Mod4+Shift+Next resize  grow  width  5 px or 5 ppt

 # split orientation
 bindsym Mod4+v split v; exec notify-send 'vertical'
 bindsym Mod4+h split h; exec notify-send 'horizontal'
 bindsym Mod4+g split v; focus parent; layout toggle split; focus child

 # wasd move
 bindsym Mod4+Shift+w move up
 bindsym Mod4+Shift+a move left
 bindsym Mod4+Shift+s move down
 bindsym Mod4+Shift+d move right

 # files
 bindsym Mod4+control+f $exec $fm
 bindsym Mod4+control+j $exec $fm Downloads

 # focus all
 bindsym Mod4+control+a $focus_all

 # move focused container to workspace and follow
 bindsym Mod4+control+1 move container to workspace $w1; workspace $w1
 bindsym Mod4+control+2 move container to workspace $w2; workspace $w2
 bindsym Mod4+control+3 move container to workspace $w3; workspace $w3
 bindsym Mod4+control+4 move container to workspace $w4; workspace $w4
 bindsym Mod4+control+5 move container to workspace $w5; workspace $w5
 bindsym Mod4+control+6 move container to workspace $w6; workspace $w6
 bindsym Mod4+control+7 move container to workspace $w7; workspace $w7
 bindsym Mod4+control+8 move container to workspace $w8; workspace $w8
 bindsym Mod4+control+9 move container to workspace $w9; workspace $w9

 # reload configuration
 bindsym Mod4+control+r $exec "make -C ~/.i3 && i3-msg $no_border restart"
 bindsym Mod4+control+Shift+r $exec "make -C ~/.i3 stable && i3-msg $no_border restart"

 # resize (precise)
 bindsym Mod4+control+i resize shrink height 1 px or 1 ppt
 bindsym Mod4+control+p resize  grow  height 1 px or 1 ppt
 bindsym Mod4+control+Prior resize shrink height 1 px or 1 ppt
 bindsym Mod4+control+Next resize  grow  height 1 px or 1 ppt
 bindsym Mod4+control+Shift+i resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+p resize  grow  width  1 px or 1 ppt
 bindsym Mod4+control+Shift+Prior resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+Next resize  grow  width  1 px or 1 ppt

 # resize (scale)
 bindsym Mod4+control+minus resize shrink height 5 px or 5 ppt; resize shrink width  5 px or 5 ppt
 bindsym Mod4+control+plus resize  grow  height 5 px or 5 ppt; resize  grow  width  5 px or 5 ppt
 bindsym Mod4+control+Shift+minus resize shrink height 1 px or 1 ppt; resize shrink width  1 px or 1 ppt
 bindsym Mod4+control+Shift+plus resize  grow  height 1 px or 1 ppt; resize  grow  width  1 px or 1 ppt

 # save layout
 bindsym Mod4+control+s exec "i3-save-tree > ~/.workspaces/stamp.json; $emacs ~/.workspaces/stamp.json"

 # start dmenu for applications
 bindsym Mod4+Shift+exclam $exec $touchpad_off; mode $def $exec $dmenu; $exec $alert $wrt

 # start dmenu for commands
 bindsym Mod4+control+e $exec $touchpad_off; mode $def $exec dmenu_run; $exec $alert $wrt

 # application menu
 bindsym Mod4+Menu $exec $touchpad_off; mode $hov exec morc_menu; $exec $alert $hov & $oklò_enable

 # no border touch
 bindsym Mod4+Shift+space $no_border

 # browse workspaces
 bindsym Mod4+Tab $exec $touchpad_off; mode $hov workspace next; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Shift+Tab $exec $touchpad_off; mode $hov workspace prev; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # default focus
 bindsym Mod4+Up $exec $touchpad_off; mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Left $exec $touchpad_off; mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Down $exec $touchpad_off; mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+Right $exec $touchpad_off; mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # oklò focus
 bindsym Mod4+o $exec $touchpad_off; mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+k $exec $touchpad_off; mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+l $exec $touchpad_off; mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+ograve $exec $touchpad_off; mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # switch workspace
 bindsym Mod4+1 $exec $touchpad_off; mode $hov workspace $w1; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+2 $exec $touchpad_off; mode $hov workspace $w2; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+3 $exec $touchpad_off; mode $hov workspace $w3; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+4 $exec $touchpad_off; mode $hov workspace $w4; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+5 $exec $touchpad_off; mode $hov workspace $w5; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+6 $exec $touchpad_off; mode $hov workspace $w6; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+7 $exec $touchpad_off; mode $hov workspace $w7; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+8 $exec $touchpad_off; mode $hov workspace $w8; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+9 $exec $touchpad_off; mode $hov workspace $w9; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # wasd focus
 bindsym Mod4+w $exec $touchpad_off; mode $hov focus up; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+a $exec $touchpad_off; mode $hov focus left; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+s $exec $touchpad_off; mode $hov focus down; $refresh_status_bar; $exec $alert $hov & $oklò_enable
 bindsym Mod4+d $exec $touchpad_off; mode $hov focus right; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # cut
 bindsym Mod4+control+x $exec $touchpad_off; mode $hov move container to workspace $wx; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # kill
 bindsym Mod4+control+w $exec $touchpad_off; mode $hov kill; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # paste
 bindsym Mod4+control+v $exec $touchpad_off; mode $hov $exec "i3-msg 'workspace --no-auto-back-and-forth $wx; move container to workspace $wx; workspace $wx'"; $refresh_status_bar; $exec $alert $hov & $oklò_enable

 # unused keys
 bindsym backslash nop
 bindsym 1 nop
 bindsym 2 nop
