 bindsym Mod4+control+minus resize shrink height 5 px or 5 ppt; resize shrink width  5 px or 5 ppt; $exec echo Mod4+control+minus >> log
 bindsym Mod4+control+plus resize  grow  height 5 px or 5 ppt; resize  grow  width  5 px or 5 ppt; $exec echo Mod4+control+plus >> log
 bindsym Mod4+control+Shift+minus resize shrink height 1 px or 1 ppt; resize shrink width  1 px or 1 ppt; $exec echo Mod4+control+Shift+minus >> log
 bindsym Mod4+control+Shift+plus resize  grow  height 1 px or 1 ppt; resize  grow  width  1 px or 1 ppt; $exec echo Mod4+control+Shift+plus >> log

 # save layout
 bindsym Mod4+control+s exec "i3-save-tree > ~/.workspaces/stamp.json; emacsclient -create-frame --alternate-editor='' ~/.workspaces/stamp.json"; $exec echo Mod4+control+s >> log

 # wasd split and move
 bindsym Mod4+control+Shift+w focus up; split v; focus down; move up; $exec echo Mod4+control+Shift+w >> log
 bindsym Mod4+control+Shift+a focus left; split v; focus right; move left; $exec echo Mod4+control+Shift+a >> log
 bindsym Mod4+control+Shift+s focus down; split v; focus up; move down; $exec echo Mod4+control+Shift+s >> log
 bindsym Mod4+control+Shift+d focus right; split v; focus left; move right; $exec echo Mod4+control+Shift+d >> log

 # f-fake fullscren
 bindsym Mod4+control+Shift+f $exec $oklò_disable; mode $def $exec "sleep 0.5; xdotool key f; sleep 0.25; i3-msg fullscreen disable"; $exec $alert $wrt; $exec echo Mod4+control+Shift+f >> log

 # start dmenu for applications
 bindsym Mod4+Shift+exclam $exec $oklò_disable; mode $def $exec $dmenu; $exec $alert $wrt; $exec echo Mod4+Shift+exclam >> log

 # start dmenu for commands
 bindsym Mod4+control+e $exec $oklò_disable; mode $def $exec dmenu_run; $exec $alert $wrt; $exec echo Mod4+control+e >> log

 # application menu
 bindsym Mod4+Menu exec morc_menu; $exec echo Mod4+Menu >> log

 # no border touch
 bindsym Mod4+Shift+space $exec $oklò_disable; mode $tch $no_border; $exec $alert $tch & $touchpad_on; $exec echo Mod4+Shift+space >> log

 # browse workspaces
 bindsym Mod4+Tab workspace next; $refresh_status_bar; $exec echo Mod4+Tab >> log
 bindsym Mod4+Shift+Tab workspace prev; $refresh_status_bar; $exec echo Mod4+Shift+Tab >> log
 bindsym Mod4+j workspace prev; $refresh_status_bar; $exec echo Mod4+j >> log
 bindsym Mod4+agrave workspace next; $refresh_status_bar; $exec echo Mod4+agrave >> log
 bindsym Mod4+Home workspace prev; $refresh_status_bar; $exec echo Mod4+Home >> log
 bindsym Mod4+End workspace next; $refresh_status_bar; $exec echo Mod4+End >> log
 bindsym Mod4+ugrave workspace back_and_forth; $refresh_status_bar; $exec echo Mod4+ugrave >> log

 # default focus
 bindsym Mod4+Up focus up; $refresh_status_bar; $exec echo Mod4+Up >> log
 bindsym Mod4+Left focus left; $refresh_status_bar; $exec echo Mod4+Left >> log
 bindsym Mod4+Down focus down; $refresh_status_bar; $exec echo Mod4+Down >> log
 bindsym Mod4+Right focus right; $refresh_status_bar; $exec echo Mod4+Right >> log

 # oklò focus
 bindsym Mod4+o focus up; $refresh_status_bar; $exec echo Mod4+o >> log
 bindsym Mod4+k focus left; $refresh_status_bar; $exec echo Mod4+k >> log
 bindsym Mod4+l focus down; $refresh_status_bar; $exec echo Mod4+l >> log
 bindsym Mod4+ograve focus right; $refresh_status_bar; $exec echo Mod4+ograve >> log

 # switch workspace
 bindsym Mod4+1 workspace $w1; $refresh_status_bar; $exec echo Mod4+1 >> log
 bindsym Mod4+2 workspace $w2; $refresh_status_bar; $exec echo Mod4+2 >> log
 bindsym Mod4+3 workspace $w3; $refresh_status_bar; $exec echo Mod4+3 >> log
