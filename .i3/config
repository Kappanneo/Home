 bindsym Mod4+8 workspace $w8; $refresh_status_bar; $exec echo Mod4+8 >> log
 bindsym Mod4+9 workspace $w9; $refresh_status_bar; $exec echo Mod4+9 >> log

 # wasd focus
 bindsym Mod4+w focus up; $refresh_status_bar; $exec echo Mod4+w >> log
 bindsym Mod4+a focus left; $refresh_status_bar; $exec echo Mod4+a >> log
 bindsym Mod4+s focus down; $refresh_status_bar; $exec echo Mod4+s >> log
 bindsym Mod4+d focus right; $refresh_status_bar; $exec echo Mod4+d >> log

 # cut
 bindsym Mod4+control+x move container to workspace $wx; $refresh_status_bar; $exec echo Mod4+control+x >> log

 # kill
